    // this.set(updated_attributes,{silent:true}); //Set the values
  },
  doSave: function doSave(opts) {
    if (this.beforeDoSave) this.beforeDoSave(); //-WD In order for calendar to save its services on first save
    var self = this;
    if (typeof opts == 'undefined') {
      opts = {};
    }
    //Whether to manipulate settings:
    if (opts.settings) {
      var settings = opts.settings; //Use settings if passed in
    } else {
      var settings = typeof APP_SETTINGS == 'undefined' ? null : APP_SETTINGS; //Otherwise look for APP_SETTINGS global
    }

    if (!opts.powr_published && this.meta.admin && this.meta.is_standalone) {
      debug()('Admin user - Blocking autosave');
      return false; //Don't Allow autosaving for ADMINS!!!
    }

    window.updateEditTopbarState && window.updateEditTopbarState('showSaving', true); //update topbar saving indicator

    //debug()("Settings is ",settings);
    if (settings) {
      debug()('Saving with settings');
      var saveButton = settings.$el.find('.saveButton');
      var saveSuccess = settings.$el.find('.saveSuccess');
      var saveError = settings.$el.find('.saveFailure');
      var progress = settings.$el.find('.saving');
      saveButton.toggleButton();
      var err = new errorHandler(saveError);
      err.clearErrors();
    }

    //Wix auto save time:
    // if (HOST == "wix" || HOST == "weebly" || opts.autoSave) {
    window.AUTO_SAVE_TIME = typeof AUTO_SAVE_TIME == 'undefined' ? 1000 : 1.1 * AUTO_SAVE_TIME;
    // }

    function autoSave() {
      //In auto-save systems, try again:
      // if (HOST == "wix" || HOST == "weebly" || opts.autoSave) {
      debug()('Save fail. Trying to save again in ' + AUTO_SAVE_TIME);
      typeof AUTO_RESAVE == 'undefined' ? null : clearTimeout(AUTO_RESAVE);
      window.AUTO_RESAVE = setTimeout(function () {
        self.doSave(opts);
      }, AUTO_SAVE_TIME);
      // }
    }

    //Custom save:
    //debug()("Doing custom save");

    //Pass everything but locals and garbage
    var post_content = jQuery.extend(true, {}, self.attributes);
    var additionalActions = post_content.locals.additionalActions || null;
    delete post_content.additionalActions;
    delete post_content.locals; //Don't send locals
    delete post_content.isPreset;
    delete post_content.colorPreset;
    delete post_content.fontPreset;
    delete post_content.designPreset;
    delete post_content.designPresetCategory;
    delete post_content.passwords;

    var post_params = {
      content: JSON.stringify(post_content),
      additionalActions: JSON.stringify(additionalActions),
      passwords: JSON.stringify(self.get('passwords')),
      original_query: originalQuery(),
      app_type: self.meta.app_type,
      view_mode: self.meta.view_mode,
      alias: self.meta.alias,
      unique_label: self.meta.unique_label,
      powr_published: opts.powr_published || !self.meta.is_standalone,
      host: window.HOST
      //meta: self.meta //Don't need this at the moment
    };

    //debug()("opts are ",opts);
    if (opts.post_params != undefined) {
      for (var key in opts.post_params) {
        post_params[key] = opts.post_params[key];
      }
    }
    //debug()("post params are ",post_params);
    var url = '/apps/' + self.meta.true_id;
    debug()('posting to url ' + url);

    ajaxController({
      url: url,
      data: post_params,
      type: 'PUT'
    }, function (r) {

      debug()('Save response: ', r);
      if (r && r.success) {
        //Set reset the parameters from response
        self.meta.saved = true; //It is now definitely saved
        debug()('Update success! Model:', self, 'Response:', r);
        var updateTopBar = function updateTopBar() {
          window.updateEditTopbarState && window.updateEditTopbarState('showSaving', false);
        };

        setTimeout(updateTopBar, 4000); //tell top bar to show 'Saved!'
        if (r.published) {
          self.isDirtyNoMoreWithCookie(); //model is no longer dirty AND unpublished cookie is destroyed
          debug()('!!!!!!!!!!!!!!!!!!!! Cookie Should Be Destroyed!');
        } else {
          self.setLocals({ isDirty: false }); // model is no longer dirty, but do NOT destroy cookie
        }

        if (settings) {
          saveButton.toggleButton();
          settings.animateSaveButton();
          settings.updateSaveStatus(); //Update the save tab
          window.updateEmbedCode(settings);
          if (HOST == 'wix' || HOST == 'weebly') {
            //Update save status
            settings.$el.find('.saveStatus .savingStatus').hide();
            settings.$el.find('.saveStatus .savedStatus').show();
            setTimeout(function () {
              settings.$el.find('.saveStatus .savedStatus').fadeOut('fast'); //Hide again in a second
            }, 1000);
          }
        }
        if (this.afterDoSave) this.afterDoSave(r);
        if (opts.callback) {
          opts.callback(r);
        }
      } else {
        debug()('UH OH...trouble saving!');
        if (settings) {
          if (r) {
            for (var i in r.errors) {
              err.addError(r.errors[i]);
            }
          };
          saveButton.toggleButton();
          err.displayErrors();
        }
        autoSave();
      }
    }.bind(this));
  }
}));

