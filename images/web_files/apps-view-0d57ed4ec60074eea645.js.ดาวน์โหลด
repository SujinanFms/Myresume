window.initializePlatformsForQA = function () {
  var platform = getURLParameter('platform') || META.external_id_type;
  if (!platform) return;
  var isBackend = /backend/.test(window.location.pathname);
  platform = _.first(platform.split('-'));

  var platformAppType = platform.toUpperCase() + '_QA_APP_TYPE';
  var platformStagingAppModel = platform + 'StagingAppModel';

  if (isBackend) {
    var platformAppType = platform.toUpperCase() + '_BACKEND_QA_APP_TYPE';
    var platformStagingAppModel = platform + 'BackendStagingAppModel';
  }

  if (typeof window[platformAppType] == 'undefined' || window[platformAppType].length == 0) return;

  if (platform == 'weebly' && isBackend) {
    window.weeblyBackendStagingAppModel = window[WEEBLY_BACKEND_QA_APP_TYPE + 'AppModel'];
    window.weeblyBackendLocalAppModel = weeblyBackendStagingAppModel;
  } else {
    window[platformStagingAppModel] = window[window[platformAppType] + 'AppModel'];

    if (platform == 'bigcommerce') {
      window.bigcommerceLocalAppModel = window[platformStagingAppModel];
    } else if (platform == 'wix') {
      window.wixLocalAppModel = window[platformStagingAppModel];
    } else if (platform == 'weebly') {
      window.weeblyLocalAppModel = window[platformStagingAppModel];
    } else {
      window.localDevAppModel = window[platformStagingAppModel];
    }
  }

  debug()('Testing ', platform, ' with - ', window[platformAppType]);
};

window.renderLogin = function () {
  $('#sign-in-modal').modal('show');
  $('#signup').removeClass('active');
  $('#signin').addClass('active');
  $('.signUpTab').removeClass('active');
  $('.signInTab').addClass('active');
};

window.renderSignup = function () {
  $('#sign-in-modal').modal('show');
};

window.getUrlParams = function () {
  var params = {};
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {
    params[key] = value;
  });
  return params;
};

// Probably not a best practice, but this gives us a cleaner way to do it now
window.updateMetaAttribute = function (key, value) {
  APP_MODEL.meta[key] = value;
  META[key] = value; //covering our asses since we use both in code base
};

window.daysBetween = function (utc_start, utc_end) {
  return Math.round(Math.abs(+utc_start - +utc_end) / 8.64e7);
};
window.endsWith = function (str, suffix) {
  return str.indexOf(suffix, str.length - suffix.length) !== -1;
};

