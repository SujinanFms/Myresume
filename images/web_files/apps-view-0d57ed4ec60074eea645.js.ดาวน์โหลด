};

window.validateUrl = function (element) {
  var validationErrorEl = element.find('.urlValidationError');
  var inputEl = element.find('input');
  var value = inputEl.val();
  var re = /^http([s]?):\/\/.*/;
  if (re.test(value) === true || value.length == 0) {
    validationErrorEl.hide();
    debug()('Url is valid');
  } else {
    validationErrorEl.html('Url is not valid!').fadeIn();
    debug()('Url is not valid');
  }
};

//Function parseVideo
window.parseVideo = function (url) {
  //Is it a youtube video?
  var regExp = /^.*(youtu.be\/|v\/|u\/\w\/|embed\/|watch\?v=)([^#\&\?]*).*/;
  var match = url.match(regExp);
  if (match && match[2].length == 11) {
    return {
      valid: true,
      type: 'youtube',
      id: match[2]
    };
  }
  //Is it a vimeo video?
  regExp = /http(s)?:\/\/(www\.)?vimeo.com\/(\d+)($|\/)/;
  match = url.match(regExp);
  if (match) {
    return {
      valid: true,
      type: 'vimeo',
      id: match[3]
    };
  }
  //Otherwise it's not valid:
  return {
    valid: false
  };
};

//Make sure url starts with http or https
window.urlFormat = function (url) {
  url = $.trim(url);
  //Can also have a mailto: or a tel: or webcal:
  if (!url.match(/^http([s]?):\/\/.*/) && !url.match(/^mailto:.*/) && !url.match(/^tel:.*/) && !url.match(/^webcal:.*/)) {
    return 'http://' + url;
  } else {
    return url;
  }
};

window.isValidUrl = function (url) {
  return (/(((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:)*@)?(((\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5]))|((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?)(:\d*)?)(\/((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)+(\/(([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)*)*)?)?(\?((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|[\uE000-\uF8FF]|\/|\?)*)?(\#((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|\/|\?)*)?$/i.test(url)
  );
};

window.collapse = function (breakpoint, larger, smaller) {
  var viewWidth = parseInt($('#appView').outerWidth());
  if (APP_MODEL.get('pluginWidth') == '100%') {
    var pluginWidth = viewWidth;
  } else {
    var pluginWidth = parseInt(APP_MODEL.get('pluginWidth'));
  }
  if (pluginWidth < breakpoint || viewWidth < breakpoint) {
    return smaller;
  } else {
    return larger;
  }
};

//Tests a string's width, given the string, size and font.
//It creates an invisible element in the DOM, then
//FONT_FUDGE_FACTOR = 0.010015;

var FONT_FUDGE_FACTOR = 0.012;

window.testStringWidth = function (str, size, font) {
  //calculates width the norm a
  var f = size + 'px ' + font;
  var o = $('<div>' + str + '</div>').css({
    position: 'absolute',
    float: 'left',
    'white-space': 'nowrap',
    visibility: 'hidden',
    font: f
  }).appendTo($('body'));
  var width = o.width();
  o.remove();
  return width;
};

window.fitText = function (elements) {
  elements.each(function () {
    //Store original font size:
    var origFontSize = $(this).data('orig-font-size'); //Get original font size
    if (origFontSize == undefined) {
      //Set original font size if it is undefined
      var origFontSize = parseInt($(this).css('fontSize'));
      $(this).data('orig-font-size', origFontSize);
    }
    //debug()("Text is : " +$(this).text()+"; and original font size is: "+origFontSize);

    var width = 0;
    var dataWidth = parseInt($(this).data('fit-text-width'));
    if (dataWidth) {
      width = dataWidth;
    } else {
      //If a fit text parent exists, then use that. Otherwise use the parent width
      if ($(this).parents('.fitTextParent').length > 0) {
        //debug()('USing fit text parent width');
        width = $(this).parents('.fitTextParent').width(); //the width that it has to fit in
      } else {
        width = $(this).parent().width(); //the width that it has to fit in
      }
    }

    var font = $(this).css('fontFamily'); //what font it's in
    var txt = $(this).text();
    if ($(this).find('br').length > 0) {
      //if it has line breaks, you gotz ta get funky
      //use a different method so as to preserve newlines
      var cloned = $(this).clone();
      cloned.find('br').replaceWith(' ');
      txt = cloned.text();
      //debug()('Txt', txt, txt.split(/\s/));
    }
    var s = $.trim(txt); //what text you have to fit in the width

    if (typeof LONGEST_WORDS == 'undefined') {
      //if LONGEST_WORDS doesn't exist, create it
      window.LONGEST_WORDS = {}; //LONGEST_WORDS is a hashtable of the widest word in a string with a given font. LONGEST_WORDS['Arial']['The longest word here is longest'] = 'longest'
    }

    if (LONGEST_WORDS[font] == null) {
      //make sure a table exists for the fiven font
      LONGEST_WORDS[font] = {}; //create a basic hastable
    }

    if (typeof TOO_LONG_WORDS == 'undefined') {
      //if TOO_LONG_WORDS doesn't exist, create it
      window.TOO_LONG_WORDS = {}; //TOO_LONG_WORDS is a hashtable that checks whether a break will be necessary. TOO_LONG_WORDS['Arial']['WayWayTooLongStringThatsTOODamnLong'] = true
    }

    if (TOO_LONG_WORDS[font] == null) {
      //make sure a table exists for the given font
      TOO_LONG_WORDS[font] = {}; //create a basic hastable
    }

    var longestWord = ''; //hold the value of the longest word that has been found
    var hasTooLongWord = TOO_LONG_WORDS[font].hasOwnProperty(s) || false; //check to see if you know you need to break a word eventually

    if (LONGEST_WORDS[font][s] == null) {
      //if you haven't already calculated the longest word, do so
      var parts = s.split(/\s/); //split the string by whitespace
      var longestWords = []; //create an array of ones it's worth considering
      var longestWordWidth = 0;
      var maxLength = 25; //anything over this is probably some kind of junk

      for (var i = 0; i < parts.length; i++) {
        //loop through all words
        if (parts[i].length >= longestWord.length - 5) {
          //if this word is long enough that it might be the widest, consider it
          if (parts[i].length > longestWord.length && parts[i].length < maxLength) {
            //if it's truly the longest, say so
            longestWord = parts[i];
          }
          longestWords.push(parts[i]);
        } else if (parts[i].length >= maxLength) {
          //tell it that it will need to break a word
          hasTooLongWord = true;
          if (!TOO_LONG_WORDS[font].hasOwnProperty(s) || TOO_LONG_WORDS[font][s].length < parts[i].length) {
            //keep track of the real longest word as well
            TOO_LONG_WORDS[font][s] = parts[i];
          }
        }
      }

      for (var i = 0; i < longestWords.length; i++) {
        //look through al the words it's worth considering
        if (longestWords[i].length >= longestWord.length - 5 && longestWords[i].length < maxLength) {
          //make sure that it's fairly long but not too long
          //debug()('Testing ' + longestWords[i]);
          var curW = computeWidth(longestWords[i], 100, font); //see how wide it is //THIS LINE TAKES AN INCREDIBLY LONG TIME FOR LOTS OF CONTENT
          if (curW > longestWordWidth) {
            //if it's the widest, tell the rest of the code that
            longestWord = longestWords[i];
            longestWordWidth = curW;
          }
        } else if (longestWords[i].length >= maxLength) {
          //tell it that it will need to break a word
          hasTooLongWord = true;
          TOO_LONG_WORDS[font][s] = TOO_LONG_WORDS[font][s] || longestWords[i];
        }
      }

      LONGEST_WORDS[font][s] = longestWord;
    } else {
      //you already know which word is longest
      longestWord = LONGEST_WORDS[font][s]; //so tell the rest of the code which word that is
    }

    if ($(this).hasClass('fitTextNoWrap')) {
      longestWord = ' ' + s + ' ';
    }

    //debug()('Longest word: ' + longestWord, 'Has too long word: ' + hasTooLongWord);

    //Find the maximumfont size
    var maxFontSize = computeMaxFontSize(longestWord, width, font);
    //if (longestWord == 'toaster') {//I only care about the limiting ones when debuggin
    //    debug()("Longest word: " + longestWord + ", max font size: " + maxFontSize + ' for width ' + width, 'Txt: ' + $(this).text(), 'Orig font size: ' + origFontSize);
    //}

    //check if the too long word is still within bounds for the current form
    if (hasTooLongWord) {
      var tooLongWordWidth = computeWidth(TOO_LONG_WORDS[font][s], Math.min(maxFontSize, parseInt(origFontSize)), font);
      hasTooLongWord = tooLongWordWidth >= width;
      //debug()(TOO_LONG_WORDS[font][s], tooLongWordWidth, width, font, maxFontSize, origFontSize);
    }

    var goodDisplay = $(this).css('display') == 'inline-block' || $(this).css('display') == 'block'; //see how strongly you need to make if break words
    var wordBreak = hasTooLongWord ? {
      '-ms-word-break': 'break-all',
      'word-wrap': 'break-word',
      'word-break': goodDisplay ? '' : 'break-all',
      '-webkit-hyphens': 'auto',
      '-moz-hyphens': 'auto',
      hyphens: 'auto'
    } : {
      /*'word-wrap': 'break-word'*/
    }; //css to make the words break properly -- but only if necessary
    if (maxFontSize < parseInt(origFontSize)) {
      //if the maximum bound is exceeded, set the font size to the maximum bound
      $(this).css($.extend({}, { fontSize: maxFontSize + 'px' }, wordBreak)); //Set the font size
      $(this).children().css($.extend({}, { fontSize: maxFontSize + 'px' }, wordBreak)); //Set the font size for children elements
    } else {
      $(this).css($.extend({}, { fontSize: origFontSize + 'px' }, wordBreak));
      $(this).children().css($.extend({}, { fontSize: origFontSize + 'px' }, wordBreak)); //Set the font size for children elements
    }
  });
};

//Calculate max font size given a string, maximum width, and font
window.computeMaxFontSize = function (strng, width, font) {
  if (typeof FONT_WIDTHS == 'undefined') {
    window.FONT_WIDTHS = {}; //Font widths is a hash of font width of a given font and string FONT_WIDTHS['arial']['A']
  }
  if (FONT_WIDTHS[font] == undefined) {
    FONT_WIDTHS[font] = {};
  }
  //Computes for entire word
  //    if(FONT_WIDTHS[font][strng] == undefined){
  //        var sumOfWidth100 = testStringWidth(strng, 100, font); //Compute width
  //        FONT_WIDTHS[font][strng] = sumOfWidth100; //Save value to globals
  //    }else{
  //        var sumOfWidth100 = FONT_WIDTHS[font][strng]; //Use global cache of font string
  //    }
  //Computes for each letter
  /*var sumOfWidth100 = 0; //Sum of the widths of the word
    for (var i = 0; i < strng.length; i++){
        var letter = strng.charAt(i);
        if (FONT_WIDTHS[font][letter] == undefined) {
            FONT_WIDTHS[font][letter] = testStringWidth(letter, 100, font);
        }
        sumOfWidth100 += FONT_WIDTHS[font][letter]; //Add to sum of widths
    }*/
  var sumOfWidth100 = computeWidth(strng, 100, font);
  //Compute max font size
  var maxFontSize = Math.floor(width / (FONT_FUDGE_FACTOR * sumOfWidth100));
  //Limit it to 100
  if (maxFontSize > 100) {
    maxFontSize = 100;
  }
  return maxFontSize;
};

window.computeWidth = function (strng, size, font) {
  //calculates width with equa s
  return 55 * strng.length * FONT_FUDGE_FACTOR * size;
  if (typeof FONT_WIDTHS == 'undefined') {
    window.FONT_WIDTHS = {}; //Font widths is a hash of font width of a given font and string FONT_WIDTHS['arial']['A']
  }
  //do it by character. Comment out this if statement to make it do it by word
  if (strng.length > 1) {
    var res = 0;
    for (var i = 0; i < strng.length; i++) {
      res += computeWidth(strng.charAt(i), size, font);
    }
    return res;
  }

  if (FONT_WIDTHS[font] == null) {
    FONT_WIDTHS[font] = {};
    FONT_WIDTHS[font][strng] = testStringWidth(strng, 100, font);
  } else if (FONT_WIDTHS[font][strng] == null) {
    FONT_WIDTHS[font][strng] = testStringWidth(strng, 100, font);
  }
  return Math.round(FONT_FUDGE_FACTOR * FONT_WIDTHS[font][strng] * size);
};

//Simple sniffer to determine mobile, only use when necessary
window.isMobile = function () {
  if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
    return true;
  } else {
    return false;
  }
};

//This can also return true for chrome apparently
window.isMobileSafari = function () {
  return navigator.userAgent.match(/(iPod|iPhone|iPad)/) && navigator.userAgent.match(/AppleWebKit/);
};

window.isWebview = function () {
  //return navigator.userAgent.search('FBAN') > -1;
  //var standalone = window.navigator.standalone;
  var userAgent = window.navigator.userAgent.toLowerCase();
  console.log('User Agent', userAgent);

  var safari = /safari/.test(userAgent);
  var ios = /iphone|ipod|ipad/.test(userAgent);
  var version = /version/.test(userAgent);

  if (ios) {
    return true;
  } else {
    return false;
  }

  //    if( ios ) {
  //        if ( !standalone && safari ) {
  //            //browser
  //        } else if ( standalone && !safari ) {
  //            //standalone
  //        } else if ( !standalone && !safari ) {
  //            //uiwebview
  //        };
  //    } else {
  //        //not iOS
  //    };
};

//test which video formats are supported
window.supportedVideoFormats = function () {
  if (window.SUPPORTED_VIDEO_FORMATS) {
    return window.SUPPORTED_VIDEO_FORMATS;
  }

  var testEl = document.createElement('video');
  var results = {
    mpeg4: false,
    h264: false,
    ogg: false,
    webm: false
  };
  if (testEl.canPlayType) {
    // Check for MPEG-4 support
    results.mpeg4 = '' !== testEl.canPlayType('video/mp4; codecs="mp4v.20.8"');

    // Check for h264 support
    results.h264 = '' !== (testEl.canPlayType('video/mp4; codecs="avc1.42E01E"') || testEl.canPlayType('video/mp4; codecs="avc1.42E01E, mp4a.40.2"'));

    // Check for Ogg support
    results.ogg = '' !== testEl.canPlayType('video/ogg; codecs="theora"');

    // Check for Webm support
    results.webm = '' !== testEl.canPlayType('video/webm; codecs="vp8, vorbis"');
  }

  window.SUPPORTED_VIDEO_FORMATS = results;

  return results;
};

//Simple window.to strip html tags
window.stripHTML = function (string) {
  return string.replace(/<(?:.|\n)*?>/gm, '');
};

window.safeAttr = function (str) {
  if (typeof str != 'string') {
    return str;
  }
  return str.replace(new RegExp('"', 'g'), "'");
};

window.capitalizeFirstLetter = function (string) {
  try {
    return string.charAt(0).toUpperCase() + string.slice(1);
  } catch (e) {
    return '';
  }
};

window.slugToName = function (string) {
  string = replaceAll('-', ' ', string);
  // return string.charAt(0).toUpperCase() + string.slice(1);
  return string.toLowerCase().split(' ').map(function (word) {
    return word.replace(word[0], word[0].toUpperCase());
  }).join(' ');
};

window.typeToName = function (string) {
  string = string.split(/(?=[A-Z])/).join(' ');
  return string.toLowerCase().split(' ').map(function (word) {
    return word.replace(word[0], word[0].toUpperCase());
  }).join(' ');
};

window.nameToUnderscore = function (string) {
  string = string.toLowerCase();
  return string.replace(/\s/g, '_');
};

window.underscoreToName = function (string) {
  string = replaceAll('_', '-', string);
  return window.slugToName(string);
};

window.toCamel = function (string) {
  if (typeof string != 'string') {
    return string;
  }
  return string.replace(/[-_]([a-z])/g, function (g) {
    return g[1].toUpperCase();
  });
};
window.toDash = function (string) {
  if (typeof string != 'string') {
    return string;
  }
  string = string.replace(/([A-Z\_])/g, function ($1) {
    return '-' + $1.toLowerCase();
  });
  return string.replace(/\_|\s/g, '').toLowerCase();
};

window.analyticsEvent = function (category, action, label) {
  try {
    if (typeof ga == 'undefined') {
      setTimeout(function () {
        window.analyticsEvent(category, action, label);
      }, 1000);
      return;
    }
    ga('powr.send', 'event', category, action, label);
  } catch (err) {}
};
window.facebookEvent = function (arg1, arg2) {
  if (window.ENVIRONMENT != 'production') {
    return; //Only track real data
  }
  if (window.fbq) {
    fbq(arg1, arg2);
  } else {
    setTimeout(function () {
      facebookEvent(arg1, arg2);
    }, 2000);
  }
};
