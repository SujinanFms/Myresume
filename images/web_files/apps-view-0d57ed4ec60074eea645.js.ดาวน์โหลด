  $('#userNotification').modal();
};

window.debug = function () {
  //return false;
  //Don't show debug in production
  if (typeof ENVIRONMENT == 'undefined' || ENVIRONMENT == 'production') {
    return function () {};
  } else {
    return console.log.bind(window.console);
  }
};

//Given an image url, attempt to return the thumbnail version
window.imageThumbUrl = function (url) {
  var imageTypes = ['large', 'medium', 'original'];
  var out_url = url;
  for (var i in imageTypes) {
    out_url = out_url.replace('/' + imageTypes[i] + '/', '/thumb/');
  }
  return out_url;
};

//Given an image url, attempt to return the version of the type you want (this works with images on POWr CDN, hopefully doesn't affect Wix or other CDNs
//Type: thumb, medium, large, original
window.imageUrl = function (url, type) {
  var imageTypes = ['thumb', 'large', 'medium', 'original'];
  var out_url = url;
  for (var i in imageTypes) {
    out_url = out_url.replace('/' + imageTypes[i] + '/', '/' + type + '/');
  }
  return out_url;
};

//Function to get base path:
window.baseUri = function () {
  var l = document.location;
  return l.protocol + '//' + l.host;
};

window.nodeUri = function (tail) {
  var tail = tail || '';
  var env = ENVIRONMENT;
  switch (env) {
    case 'development':
      return 'https://localhost:3001' + tail;
    case 'staging':
      return 'https://powr-outlet-staging.herokuapp.com' + tail;
    case 'production':
      return 'https://powr-outlet.herokuapp.com' + tail;
  }

  return 'https://powr-outlet-staging.herokuapp.com'; //if all else fails, throw it on staging
};

window.imageResizeUri = function (url) {
  var url = url || '';
  var env = ENVIRONMENT;
  switch (env) {
    case 'development':
      return 'https://q4rtf38bc7.execute-api.us-east-1.amazonaws.com/dev/resize?url=' + url;
    case 'staging':
      return 'https://xfxdcpb8d7.execute-api.us-east-1.amazonaws.com/staging/resize?url=' + url;
    case 'production':
      return 'https://q4qge3xxdk.execute-api.us-east-1.amazonaws.com/production/resize?url=' + url;
  }

  return 'https://q4rtf38bc7.execute-api.us-east-1.amazonaws.com/dev/resize?url=' + url;
};

