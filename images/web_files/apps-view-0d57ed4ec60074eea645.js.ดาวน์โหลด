window.loadNodeImages = function (el) {
  var ext;
  if (typeof el == 'undefined') {
    el = $(document);
  }
  el.find('[data-node-image], [data-node-background-image]').each(function () {
    var $current_el = $(this);
    var val = $current_el.attr('data-node-image') || $current_el.attr('data-node-background-image');
    //debug()("Node image val: "+val);
    var url = '';
    if (val) {
      ext = decodeURI(val).split('.').pop();
      if (_.isEmpty(val) || !_.include(['png', 'jpg', 'jpeg', 'gif', 'tiff', 'webp'], ext.toLowerCase()) || val.search(amazonawsUriPrefix()) == -1 || val.search('amazonaws.com') == -1) {
        url = val;
      } else {
        if ($current_el.data('node-image-size')) {
          size = $current_el.data('node-image-size');
        } else {
          if ($current_el.closest('.nodeImageParent').length > 0) {
            var parent = $current_el.closest('.nodeImageParent');
          } else {
            var parent = $current_el.parent();
          }
          var w = parent.width();
          if (w == undefined || w == 0 || w >= 1000) {
            size = 'extraLarge';
          } else if (w >= 600) {
            size = 'large';
          } else if (w >= 150) {
            size = 'medium';
          } else {
            size = 'small';
          }
        }

        var sizes = {
          small: 100,
          medium: 500,
          large: 1000,
          extraLarge: 2000
        };
        var sizeValue = sizes[size];
        var oldNaming = val.split('amazonaws.com/')[1].indexOf('.') !== -1;
        if (oldNaming) {
          var imageIdWithFilename = val.split('amazonaws.com')[1];
          var imageIdWithoutExtension = imageIdWithFilename.replace('.' + ext, '');
          var amazonStoragePrefix = 'https://' + amazonawsUriPrefix() + '.s3.amazonaws.com';
          url = '' + amazonStoragePrefix + imageIdWithoutExtension + '_' + sizeValue + '.' + ext;
        } else {
          url = val + '_' + sizeValue;
        }
      }
    }
    if ($current_el.attr('data-node-background-image')) {
      $current_el.css('background-image', 'url(' + url + ')');
    } else {
      $current_el.attr('src', url);
    }
  });
};

window.getResizedImageUrl = function (val, size) {
  if (_.isEmpty(val) || val.search(amazonawsUriPrefix()) == -1 || val.search('amazonaws.com') == -1) {
    return val;
  }
  var ext = decodeURI(val).split('.').pop();
  var sizes = {
    small: 100,
    medium: 500,
    large: 1000,
    extraLarge: 2000
  };
  var sizeValue = sizes[size];
  var oldNaming = val.split('amazonaws.com/')[1].indexOf('.') !== -1;
  if (oldNaming) {
    var imageIdWithFilename = val.split('amazonaws.com')[1];
    var imageIdWithoutExtension = imageIdWithFilename.split('.')[0];
    var amazonStoragePrefix = 'https://' + amazonawsUriPrefix() + '.s3.amazonaws.com';
    if (ext === 'svg') {
      url = '' + amazonStoragePrefix + imageIdWithoutExtension + '.' + ext;
    } else {
      url = '' + amazonStoragePrefix + imageIdWithoutExtension + '_' + sizeValue + '.' + ext;
    }
  } else {
    url = val + '_' + sizeValue;
  }
  return url;
};

window.getOriginalImage = function (val) {
  if (_.isEmpty(val) || val.search('amazonaws') == -1 || val.search('powr') == -1) {
    return val;
  }

  var oldNaming = val.split('amazonaws.com/')[1].indexOf('.') !== -1;
  var url = val,
      ext = void 0;

  if (oldNaming) {
    var urlSplitted = val.split('.');
    ext = urlSplitted.pop();
    url = urlSplitted.join('.');
  }

  var size = url.match(/.*(_100|_500|_1000|_2000)$/);

  if (size) {
    url = url.replace(size[1], '');
  }
  if (oldNaming) {
    url = url + '.' + ext;
  }

  return url;
};

window.hashEmail = function (email, type) {
  if (typeof email != 'string' || email.search('@') == -1 || email.search('.') == -1 || typeof CryptoJS == 'undefined') {
    return '';
  }
  email = $.trim(email);
  //debug()("Email is: "+email);
  var md5 = CryptoJS.MD5(email.toLowerCase()).toString(CryptoJS.enc.Hex);
  var sha1 = CryptoJS.SHA1(email.toLowerCase()).toString(CryptoJS.enc.Hex);
  var sha256 = CryptoJS.SHA256(email.toLowerCase()).toString(CryptoJS.enc.Hex);
  if (type === 'SHA256') {
    return sha256;
  } else {
    return 'it=4&iv=' + md5 + '&it=4&iv=' + sha1 + '&it=4&iv=' + sha256;
  }
};

window.loadCrypto = function (callback) {
  if (typeof CRYPTO_LOADED == 'undefined') {
    $.ajax({
      url: '/public-assets/crypto.min.js',
      cache: true,
      dataType: 'script',
      success: function success() {
        //debug()("Crypto loaded");
        window.CRYPTO_LOADED = true;
        if (typeof callback != 'undefined') {
          callback();
        }
      }
    });
  } else {
    if (typeof callback != 'undefined') {
      callback();
    }
  }
};

window._deliverHash = function (email) {
  var hash = hashEmail(email, 'SHA256');
  if (hash.length == 0) {
    return;
  }
  if (typeof DELIVER_HASH == 'undefined') {
    // debug()("Delivering hash");
    window.DELIVER_HASH = true;
    var el = document.createElement('script');
    el.async = true;
    el.src = '//pippio.com/api/sync?pid=7876&it=4&iv=' + hash;
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(el, s);
    analyticsEvent('hashed_data', hash);
    $(document.body).append('\n          <iframe name="_rlcdn" id="_rlcdn" width=0 height=0 frameborder=0 src="https://ejp.rlcdn.com/472056.html?s256=' + hash + '"></iframe>\n          ');
    debug()('iframe added');
  }
};

window.deliverHash = function (email) {
  // debug()("Deliver hash called");
  if (!email || email.length == 0) {
    return;
  }
  if (typeof IS_EU != 'undefined' && IS_EU && getCookie('cookie_acceptance') != 'accepted') {
    // debug()("Euro who has not accepted");
    return;
  }
  if (getCookie('anonymized_user') != undefined) {
    // debug()("Deliver hash cookie already set");
    return;
  } else {
    // debug()("setting anonymized_user cookie");
    setCookie('anonymized_user', true, 1);
  }
  // debug()("Call loadCrypto");
  loadCrypto(function () {
    // debug()("Loadcrypto callback. Email is "+email);
    _deliverHash(email);
  });
};

window.uniqueLabel = function (opts) {
  if (typeof opts == 'undefined') {
    opts = { temp_label: false }; //Will create a new label every time
  }
  if (opts.temp_label == false) {
    if (typeof APP_MODEL != 'undefined' && APP_MODEL.meta && APP_MODEL.meta.unique_label) {
      //Return the saved value if it exists
      return APP_MODEL.meta.unique_label;
    } else if (typeof UNIQUE_LABEL != 'undefined') {
      return UNIQUE_LABEL; //Use the global unique label
    }
  }
  var unique_label = 'xxxxxxxx_'.replace(/[x]/g, function (c) {
    var r = Math.random() * 16 | 0,
        v = c == 'x' ? r : r & 0x3 | 0x8;
    return v.toString(16);
  }) + new Date().getTime();

  if (opts.temp_label == false) {
    if (typeof APP_MODEL != 'undefined' && APP_MODEL.meta) {
      //Set to the model for next time
      APP_MODEL.meta.unique_label = unique_label;
    } else {
      window.UNIQUE_LABEL = unique_label; //Set a global unique label
    }
  }
  return unique_label;
};

window.loadFile = function ($file_wrapper, file_url) {
  var name = file_url.substring(file_url.lastIndexOf('%2F') + 3); //If file_file_name is not present than parse it from the url
  $file_wrapper.find('.fileName').val(name);
  $file_wrapper.find('.fileUrl').val(file_url);
  //Update text
  $file_wrapper.find('.existingTitle').html('Current file: ' + '<span>' + name + '</span>').attr('href', file_url);
  // $file_wrapper.find('.existingTitle').html('Current file: ' + name); //Write the file name
  $file_wrapper.find('.addFile span').html(' ' + POWr.simpleCopies('replace_file')); //Update text
  $file_wrapper.find('.noImage').addClass('hid');
};

window.loadImage = function ($image_wrapper, img_url) {
  $image_wrapper.find('img').attr('src', img_url);
  $image_wrapper.find('input').val(img_url);
  $image_wrapper.fadeIn('fast'); //Show image
  $image_wrapper.find('.addImage').html("<i class='fa fa-refresh'></i> Replace"); //Update text
  $image_wrapper.find('.noImage').addClass('hid');
};

window.add_container = function (type, $add_container_button, callback) {
  var self = this;
  if ($add_container_button.attr('active') === 'false') {
    return false;
  }
  if ($add_container_button.hasClass('premium') || $add_container_button.hasClass('ignore')) {
    return false;
  }
  if ($(this).data('add-target') === '.modWrapper') {
    var prototypeWrapper = $($(this).data('add-target'));
  } else if ($add_container_button.parents('.multiAdd').length > 0) {
    // var $prototypeWrapper = $add_container_button.parents('.multiAdd').prev('.prototype-wrapper'); //Handle multi add
    var $prototypeWrapper = $('.prototype-wrapper');
  } else {
    var $prototypeWrapper = $add_container_button.prev('.prototype-wrapper'); //Handle regular add
  }
  $prototypeWrapper.addPrototype(function ($prototype_el) {
    $prototype_el.find('.innerPanelToggle').click();
    $prototype_el.children('.mediaBar').find('.toggleSpace').click();
    APP_SETTINGS.updatePremiumContent(); //Handles limits on number of elements
    if ($('.add[data-type=image]').hasClass('premium')) {
      // requires module show up msg otherwise it disappears quickly
      $('.add[data-type=image]').popover('show');
    }
    APP_SETTINGS.afterAdd($prototype_el, $add_container_button);
    APP_SETTINGS.update();
    APP_SETTINGS.initializePickers();
    if (type === 'file') {
      var $container_wrapper = $prototype_el.find('.fileInput');
    } else if (type === 'image') {
      var $container_wrapper = $prototype_el.find('.imageWrapperInner');
    }
    callback({ success: true, $file_wrapper: $container_wrapper });
  }, { speed: 'instant' });
};

window.add_image_container = function (type, namespace, callback) {
  var $prototypeWrapper = $('.prototype-wrapper');
  $prototypeWrapper.addPrototype(function ($prototype_el) {
    var jqueryElementMock = {
      data: function data() {
        return type;
      }
    };
    APP_SETTINGS.afterAdd($prototype_el, jqueryElementMock);
    var $container_wrapper = $prototype_el.find('input[data-namespace="' + namespace + '"]');
    callback({ success: true, $file_wrapper: $container_wrapper });
  }, { speed: 'instant' });
};

window.initialize_manager = function (type, template, app_id, hideUrl) {
  var file_html = _.template(template.html())({
    opts: {
      name: type,
      capitalName: type.humanize,
      accept: '*',
      hideUrl: hideUrl
    }
  });
  template.after(file_html);
  if (type === 'file') {
    window.FILE_MANAGER = createFileManager({
      app_id: app_id,
      el: $('#fileManager')
    });
  } else {
    window.IMAGE_MANAGER = new imageManager({
      app_id: app_id,
      el: $('#imageManager')
    });
  }
};

window.show_file_manager = function (container_required, $add_document_button) {
  FILE_MANAGER.show(function (r) {
    //Call file manager
    debug()('Response from file manager:', r);
    if (r.success) {
      for (var i = 0; i < r.images.length; i++) {
        var url = r.images[i].medium_url; //Grab first image from selector. Note use medium_url as default, this can be changed later
        if (container_required === true) {
          add_container('file', $add_document_button, function (response) {
            var $file_wrapper = response.$file_wrapper;
            // upload_img_into_container(r.images[i].medium_url);
            loadFile($file_wrapper, r.images[i].medium_url);
            APP_SETTINGS.update();
          });
        } else {
          loadFile($add_document_button, r.images[i].medium_url);
          APP_SETTINGS.update();
        }
      }
    }
  });
};

window.show_image_manager = function (container_required, $add_image_button, radio_image_upload, namespace) {
  if (typeof HOST != 'undefined' && HOST === 'wix') {
    Wix.Settings.openMediaDialog(Wix.Settings.MediaType.IMAGE, true, function (data) {
      //Handle IMAGE uploads
      debug()('Wix image data: ', data);
      if (typeof WIX_IMAGE_RESPONSE_LIMITER !== 'undefined' && WIX_IMAGE_RESPONSE_LIMITER) {
        return; //Don't do anything
      } else {
        window.WIX_IMAGE_RESPONSE_LIMITER = true;
        setTimeout(function () {
          WIX_IMAGE_RESPONSE_LIMITER = false; //Reset in a few seconds
        }, 5000);
      }
      for (var i = 0; i < data.length; i++) {
        var rel_url = data[i].relativeUri; //This is a relative url like 300a449823eb962ddf8e0b253407a2d4.jpg
        var url = Wix.Utils.Media.getResizedImageUrl(rel_url, 1200, 1200);
        if (container_required === true) {
          // pilar
          add_container('image', $add_image_button, function (response) {
            var $image_wrapper = response.$file_wrapper;
            loadImage($image_wrapper, url);
            APP_SETTINGS.update();
          });
        } else {
          loadImage($add_image_button, r.images[i].medium_url);
          APP_SETTINGS.update();
        }
      }
    });
  } else {
    IMAGE_MANAGER.show(function (r) {
      //Call image manager
      if (r.success) {
        for (var i = 0; i < r.images.length; i++) {
          var url = r.images[i].medium_url; //Grab first image from selector. Note use medium_url as default, this can be changed later
          if (radio_image_upload) {
            var input = $('#radio-custom-image').find('input');
            var image = $('#radio-custom-image').find('img');
            $(image).attr('src', url);
            $(input).val(url);
            $(input).attr('name', namespace);
            $(input).closest('.radioImageContainer').removeClass('initialized');
            $add_image_button.before($('#radio-custom-image').html());
            APP_SETTINGS.initializeCheckboxes();
            APP_SETTINGS.update();
            $('.' + namespace + 'Select').find('#custom--upload').hide();

            $('.replaceCustomUpload').click(function () {
              $(this).parent('.customRadioContainer').remove();
              $('.' + namespace + 'Select').find('#custom--upload').show();
            });
          } else if (container_required === true) {
            add_container('image', $add_image_button, function (response) {
              var $image_wrapper = response.$file_wrapper;
              loadImage($image_wrapper, r.images[i].medium_url);
              APP_SETTINGS.update();
            });
          } else {
            loadImage($add_image_button, r.images[i].medium_url);
            APP_SETTINGS.update();
          }
        }
      }
    });
  }
};

