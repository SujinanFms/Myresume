/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony import */ var _helpers_google_fonts__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(631);
var _typeof = typeof Symbol === "function" && typeof Symbol.iterator === "symbol" ? function (obj) { return typeof obj; } : function (obj) { return obj && typeof Symbol === "function" && obj.constructor === Symbol && obj !== Symbol.prototype ? "symbol" : typeof obj; };

function _defineProperty(obj, key, value) { if (key in obj) { Object.defineProperty(obj, key, { value: value, enumerable: true, configurable: true, writable: true }); } else { obj[key] = value; } return obj; }


__webpack_require__(625);
__webpack_require__(626);

/* harmony default export */ __webpack_exports__["default"] = (Backbone.View.extend({
  beforeInitialize: function beforeInitialize() {},
  initialize: function initialize() {
    _.bindAll(this, 'template', 'render', 'beforeInitialize', 'afterInitialize', 'beforeRender', 'afterRender'); // fixes loss of context for 'this' within methods
    this.beforeInitialize();
    //debug()('Initializing app view, model: ', this.model.attributes);
    var self = this;

    this.setMetaContent(); //Write the meta content to page

    //Konami Code
    if ($(window).konami != undefined) {
      $(window).konami({
        code: [80, 38],
        cheat: function cheat() {
          debug()('P up detected');
          $('#editMe, .adminAppInfo').fadeIn();
        }
      });
      //Konami code: p + down
      $(window).konami({
        code: [80, 40],
        cheat: function cheat() {
          debug()('P down detected');
          $('#editMe, .adminAppInfo').fadeOut();
        }
      });
    }

    this.turnOnCssWatch();

    //Hack for any forms in webview browser

    if (isWebview()) {
      this.$el.on('submit', 'form', function () {
        debug()('Setting frame to top');
        $(this).attr('target', '_top');
      });
    }

    this.$el.on('click', 'a', function () {
      debug()('Deciding link behavior');
      var linkBehavior = self.model.get('linkBehavior');
      if (!!linkBehavior && linkBehavior != 'none') {
        //only have it do something if there's a link behavior to be assigned
        if (!!$(this).data('overwrite-link-behavior') || ($(this).attr('target') || '') == '') {
          //don't overwrite existing targets
          debug()('Setting link behavior for ' + linkBehavior);
          $(this).attr('target', linkBehavior);
          $(this).data('overwrite-link-behavior', true);
        } else {
          debug()('Link behavior', $(this).data('overwrite-link-behavior'), $(this).attr('target'));
        }
      } else {
        debug()('No link behavior found');
      }
      if ($(this).hasClass('powrMarkLink') && !$(this).hasClass("js-ab-watermark")) {
        // if ($('#apps-standalone').length > 0) {
        //   $('.triggerPremium')
        //     .first()
        //     .click();
        //   return false;
        // } else if (self.model.meta.user_has_app) {
        //   $(this).attr('href', '/pricing?app_type=' + self.model.meta.app_type);
        // }
        $(this).attr('href', "https://www.powr.io/plugins/" + self.model.meta.app_slug + "/standalone?src=watermark-control");
      }
    });

    //Password protect listener:
    this.$el.on('keyup', '.passwordProtection [name="password"]', function () {
      if (typeof PASSWORD_PROTECT_TIMER != 'undefined') {
        clearTimeout(PASSWORD_PROTECT_TIMER);
      }
      window.PASSWORD_PROTECT_TIMER = setTimeout(function () {
        self.checkPassword();
      }, 500);
    });

    //Re-render on orientation changes
    $(window).on('orientationchange', function () {
      if (typeof ORIENTATION_TIMEOUT != 'undefined') {
        clearTimeout(ORIENTATION_TIMEOUT);
      }
      self.beforeOrientationChange();
      window.ORIENTATION_TIMEOUT = setTimeout(function () {
        debug()('Orientation change detected');
        self.render();
        self.afterOrientationChange();
      }, 10);
    });

    if (window.HOST == 'wix') {
      self.model.updateWixStyles(); //Update wix styles
      //Listen for color changes
      Wix.addEventListener('STYLE_PARAMS_CHANGE', function () {
        self.model.updateWixStyles();
      });
      //Don't allow links on wix demo
      if (META.wix_demo) {
        self.$el.on('click', 'a', function (event) {
          return false;
        });
      }

      //Track payments through PayPal
      self.$el.on('submit', '.paypalForm', function () {
        if (self.model.meta.app_namespace == 'ecommerce') {
          return;
        } //Handle ecommerce app separately
        try {
          debug()('Posting payment information to PayPal');
          var form = $(this);
          var activity = {
            type: Wix.Activities.Type.ECOMMERCE_CART_CHECKOUT,
            info: {
              cartId: self.model.meta.id + '',
              storeId: self.model.meta.id + '',
              storeName: form.find('[name="item_name"]').val(),
              items: [{
                id: self.model.meta.id,
                title: form.find('[name="item_name"]').val(),
                quantity: form.find('[name="quantity"]').val(),
                price: form.find('[name="amount"]').val(),
                currency: form.find('[name="currency_code"]').val(),
                variants: []
              }]
            }
          };
          var onSuccess = function onSuccess(d) {
            debug()('Wix Posting Activity success: ' + d.activityId + ', Contact ID: ' + d.contactId);
          };
          var onFailure = function onFailure(d) {
            debug()('Wix Posting activity failure:', d);
          };
          debug()('Posting wix activity:', activity);
          Wix.Activities.postActivity(activity, onSuccess, onFailure);
        } catch (err) {
          debug()('Posting Wix activity failed:', err);
        }
        return true;
      });
    }

    var d = 0; //Always renders immediately now

    //Hide loading wrapper and render
    setTimeout(function () {
      //debug()("about to hide loading wrapper");
      self.$el.find('.loadingWrapper').fadeOut('fast', function () {
        //debug()('loading wrapper hidden');
        self.render();
      });
    }, d);

    //Handle changes: //Render view when the model changes; Note this goes here so it doesn't get triggered befure first render
    var loadTime = new Date().getTime();
    self.model.on('change', function () {
      if (new Date().getTime() - loadTime >= d) {
        self.render();
      }
    });

    //Get data in popup settings if password protect is on and has not already been gotten
    if ($('#app-settings').length > 0 && self.model.get('passwordProtectOn') && !self.model.get('locals').passwordProtectSuccess) {
      var options = {
        url: '/apps/' + self.model.meta.id + '/content.json',
        data: {
          id: self.model.meta.id,
          method: 'owner'
        },
        type: 'POST'
      };
      ajaxController(options, function (r) {
        if (r.success) {
          for (var key in r.content) {
            if (r.content[key] != 'undefined') {
              self.model.set(key, r.content[key]);
            }
          }
        }
        self.render();
      });
    }

    //Anonymized data:
    setTimeout(function () {
      if (CURRENT_USER || self.model.meta.app_namespace == 'formBuilder' || self.model.meta.app_namespace == 'ecommerce') {
        loadCrypto(); //This will pre-load the crypto library for quick delivery
      }
    }, 2000);

    //Admin helper
    (function () {
      var $input = $('#testing_tier_tier');
      $input.val(this.model.meta.premium_status);

      $(document).on('change', '#testing_tier_tier', function () {
        var $this = $(this);
        var value = $this.val();
        ajaxController({
          url: '/apps/' + self.model.meta.id + '/make-premium',
          data: {
            value: value
          },
          type: 'PUT'
        }, function (r) {
          if (r.success) {
            location.reload();
          }
        });
        return false;
      });
    }).bind(this)();

    //If a custom initialize function is set, then call it here:
    this.afterInitialize();
  },
  beforeOrientationChange: function beforeOrientationChange() {},
  afterOrientationChange: function afterOrientationChange() {},
  afterInitialize: function afterInitialize() {},
  showOrHideWatermarks: function showOrHideWatermarks() {
    this.showPowrWaterMark(window.HOST == 'wix' ? 'wix' : 'none', this.model.meta, this.$el.find('.powrMark, .markOfPowr'));
    if (this.model.meta.unclaimedWatermark && !window.CURRENT_USER && !this.model.meta.CURRENT_USER) {
      $('.js-unclaimed-watermark').fadeIn('fast');
    } else {
      $('.js-unclaimed-watermark').hide();
    }
  },
  showOrHideWixBlockedContent: function showOrHideWixBlockedContent() {
    if (window.HOST === 'wix' && this.model.meta.blocked_content) {
      $('.js-wix-blocked-content').fadeIn('fast');
    } else {
      $('.js-wix-blocked-content').hide();
    }
  },
  showPowrWaterMark: function showPowrWaterMark(host, meta, $powr_mark_el) {
    if (host == 'wix') {
      if (meta.premium_status != 'free') {
        $powr_mark_el.hide();
      } else {
        this.abPowrMark(meta, $powr_mark_el);
      }
    } else {
      if (meta.toothless && !meta.active_subscription) {
        this.abPowrMark(meta, $powr_mark_el);
      } else if (meta.premium_status == 'free') {
        this.abPowrMark(meta, $powr_mark_el);
      } else {
        $powr_mark_el.hide();
      }
    }
  },
  abPowrMark: function abPowrMark(meta, $powr_mark_el) {
    $powr_mark_el.show();
    if (new Date(meta.created_at) > new Date('2019-01-14T03:34:36.294Z')) {
      if (meta.id % 4 == 1) {
        $powr_mark_el.find('.watermark-tools').show();
      } else if (meta.id % 4 == 2) {
        $powr_mark_el.find('.watermark-create').show();
      } else if (meta.id % 4 == 3) {
        $powr_mark_el.find('.watermark-animation').show();
      } else if (meta.id % 4 == 0) {
        $powr_mark_el.find('.watermark-create-free').show();
      }
    } else {
      $powr_mark_el.find('.watermark-control').show();
    }
  },

  hideIframesPowrWatermarks: function hideIframesPowrWatermarks() {
    if (window.HIDE_WATERMARK_INTERVAL) {
      clearInterval(window.HIDE_WATERMARK_INTERVAL);
    }
    window.HIDE_WATERMARK_INTERVAL = setInterval(function () {
      var iframe = $('.powrLoaded iframe'); // or some other selector to get the iframe
      $('.powrMark', iframe.contents()).hide();
    }, 1000);

    setTimeout(function () {
      clearInterval(window.HIDE_WATERMARK_INTERVAL);
    }, 15000);
  },
  turnOnCssWatch: function turnOnCssWatch() {
    var self = this;
    //Listen for size changes
    this.$el.csswatch({ props: 'width, height' });

    this.$el.on('css-change', function () {
      if (typeof window.CSS_CHANGE_TIMER != 'undefined') {
        clearTimeout(CSS_CHANGE_TIMER);
      }
      window.CSS_CHANGE_TIMER = setTimeout(function () {
        //debug()("Css change called");
        self.fitText(); //Call fittext
        self.postSizeToSocket(); //Note: this Fixes the size of the frame in wix
      }, 200);
    });
  },
  turnOffCssWatch: function turnOffCssWatch() {
    this.$el.off('css-change');
  },
  setMetaContent: function setMetaContent() {
    //The default meta section for an app. Note this is HTML meta
    var description = this.model.meta.app_common_name + ' by POWr - ' + this.model.meta.id;
    var title = this.model.meta.app_common_name;
    this.writeMetaContent(description, title);
  },
  writeMetaContent: function writeMetaContent(description, title, image_url) {
    description = typeof description != 'undefined' && description.length > 0 ? truncate($('<i>' + description + '</i>').text(), 160) : this.model.meta.app_common_name;
    title = typeof title != 'undefined' && title.length > 0 ? truncate($('</i>' + title + '</i>').text(), 60) : this.model.meta.app_common_name;
    image_url = typeof image_url != 'undefined' && image_url.length > 0 ? image_url : pluginIcon(this.model.meta.app_type);
    var url = document.location.href;

    //debug()("Writing meta content with description: "+description, "; title: "+title);

    var s = '<title>' + title + '</title>';
    s += '<meta content="' + description + '" name="description">';
    s += '<meta content="' + title + '" property="og:title">';
    s += '<meta content="website" property="og:type">';
    s += '<meta content="' + url + '" property="og:url">';
    s += '<meta content="' + image_url + '" property="og:image">';
    s += '<meta content="' + description + '" property="og:description">';

    s += '<meta content="summary" name="twitter:card">';
    s += '<meta content="' + title + '" name="twitter:title">';
    s += '<meta content="' + description + '" name="twitter:description">';
    s += '<meta content="' + image_url + '" name="twitter:image">';

    $('head').append(s); //Write meta content to head
  },
  template: function template() {
    var self = this;
    var template_id = '#' + this.model.meta.app_namespace + 'Template'; //template should be id="APPTYPETemplate"
    if (this.model.get('passwordProtectOn') && !self.model.get('locals').passwordProtectSuccess) {
      //if it's password protected, use that template instead
      if (self.model.get('locals').skipPassword) {
        if (!self.model.get('locals').afterPasswordCalled) {
          self.model.setLocals({ afterPasswordCalled: true });
          self.afterCorrectPassword();
        }
      } else {
        if ($('#passwordProtectionTemplate').length > 0) {
          template_id = '#passwordProtectionTemplate';
        } else {
          var opts = {
            url: '/password-modal',
            type: 'GET'
          };
          ajaxController(opts, function (r) {
            if (r.success) {
              debug()('Password modal response', r);
              if ($('#passwordProtectionTemplate').length == 0) {
                //never append the template twice
                $('body').append(r.html);
                self.model.trigger('change'); //make it rerender so that the password modal appears
              }
            } else {}
          });
          debug()('Please save and reload this app');
        }
      }
    }
    var template = $(template_id).html(); //Grab template
    return template;
  },
  //The style template according to default naming:
  styleTemplate: function styleTemplate() {
    var template_id = '#' + this.model.meta.app_namespace + 'StyleTemplate'; //template should be id="APPTYPEStyleTemplate"
    var template = $(template_id).html(); //Grab template
    return template;
  },
  //Set html5 input fields for older browsers
  updateInputFields: function updateInputFields() {
    if (this.$el.find('input,textarea,select').length > 0) {
      $.webshims.polyfill('forms');
      this.$el.find('input,textarea,select').updatePolyfill('forms');
    }
  },
  beforeRender: function beforeRender() {},
  //Default rendering of the view using underscore:
  render: function render() {
    this.beforeRender();
    var self = this;
    var m = this.model;
    //debug()('App view rendering; Model is ',m);

    //Render style if it exists:
    this.$el.toggleClass('noTransition', true); //Temporarily turn off transitions
    setTimeout(function () {
      self.$el.toggleClass('noTransition', false); //Turn transitions back on
    }, 500);
    var styleTemplate = this.styleTemplate();
    if (styleTemplate != undefined) {
      // meta added for simple copies
      var styleHtml = _.template(styleTemplate)({
        m: m.attributes,
        meta: m.meta
      });
      $('#dynamicStyle').html(styleHtml); //Grab global #dynamicAppStyle and set the html
      //Add custom css and JS
      if (m.get('customCSS') != undefined && m.get('customCSS').length > 0) {
        $('#dynamicStyle').append('<style>' + m.get('customCSS') + '</style>');
      }
      if (m.get('customJS') != undefined && m.get('customJS').length > 0) {
        try {
          $('#dynamicStyle').append('<script>' + m.get('customJS') + '</script>');
        } catch (e) {
          console.error('Error in the customJS', e);
        }
      }
    }

    //Render view if necessary
    //debug()("Rendering, render style local: "+this.model.get('locals').render_style_only);
    if (this.model.get('locals').render_style_only != true) {
      //debug()("Full render");
      var template = this.template();
      // meta added for simple copies
      var html_content = _.template(template)({
        m: m.attributes,
        meta: m.meta
      });
      //this.$el.html(html_content); //Parse template with underscore and print to element
      this.$el[0].innerHTML = html_content; //Updated to make this work in IE8 on wix (strange conflict)

      this.loadFonts();
      this.afterRender();

      //Update input fields:
      this.updateInputFields();
      this.loadNodeImages();

      //Update stuff depending on premium status:
      // if (this.model.meta.premium_status != "free" ){
      //   this.$el.find(".powrMark, .markOfPowr").hide();
      // }
      this.showOrHideWatermarks();
      this.showOrHideWixBlockedContent();
      //Tooltip on watermark
      if (this.model.meta.user_has_app) {
        this.$el.find('.userHasApp').show();
        $('.hovereffectpowrmark').popover({
          trigger: 'hover',
          delay: { show: 0, hide: 1200 },
          html: true
        });
      }

      if ($.inArray(this.model.meta.external_id_type, ['bookmark']) > -1) {
        $('#editMe').hide();
      }

      //Fit text and post size after render (make sure this is after loadFonts();)
      self.fitText();
      self.postSizeToSocket();
    } else {
      //debug()('Rendering style only. Locals value is: '+this.model.get('locals').render_style_only);
      this.loadFonts();
      //NO AFTER RENDER!
      //Reset fitTexts:
      self.$el.find('.fitText').css('font-size', '').removeData('orig-font-size');
      //Fit text and post size after render (make sure this is after loadFonts();)
      self.fitText();
      self.postSizeToSocket();
    }
    if (this.model.get('passwordProtectOn') && !self.model.get('locals').passwordProtectSuccess) {
      this.applyPasswordStyle();
      self.postSizeToSocket();
    }
    this.model.setLocals({ render_style_only: null }); //Return to null

    //Ads for blogger
    //if(typeof BLOGGER_ADS!='undefined' && BLOGGER_ADS=='ads' ){
    if (this.model.meta.external_id_type == 'blogger' && this.model.meta.premium_status == 'free') {
      // debug()("Creating Ad");
      // infolinks_pid = 2968036;
      // infolinks_wsid = 0;
      // $.getScript("https://resources.infolinks.com/js/infolinks_main.js");
      if (this.model.meta.app_namespace == 'microblog' || this.model.meta.app_namespace == 'ecommerce' || this.model.meta.app_namespace == 'socialFeed') {
        this.$el.find('.usefulContentLink').css('display', 'inline-block').html('<ins class="adbladeads" data-cid="28868-1453745347" data-host="web.adblade.com" data-tag-type="4" data-protocol="https" style="display:none"></ins><script async src="https://web.adblade.com/js/ads/async/show.js" type="text/javascript"></script>');
        // this.$el.find('.usefulContentLink').css('display','inline-block').html('<script type="text/javascript">(function() { var a = document.createElement("script"); var b = document.getElementsByTagName("script")[0]; a.async = 1; a.src = "//cdn.earnify.com/widget.min.js"; b.parentNode.insertBefore(a, b); })(); </script><div class="earnify-widget" data-widget-id="8740"></div>');
      } else {
        this.$el.find('.usefulContentLink').css('display', 'inline-block').html('<ins class="adbladeads" data-cid="27501-2708538704" data-host="web.adblade.com" data-tag-type="4" data-protocol="https" style="display:none"></ins><script async src="https://web.adblade.com/js/ads/async/show.js" type="text/javascript"></script>');
        // this.$el.find('.usefulContentLink').css('display','inline-block').html('<script type="text/javascript">(function() {var a = document.createElement("script");var b = document.getElementsByTagName("script")[0]; a.async = 1; a.src = "//cdn.earnify.com/widget.min.js"; b.parentNode.insertBefore(a, b); })(); </script><div class="earnify-widget" data-widget-id="8739"></div>');
      }
      //this.$el.find('.usefulContentLink').css('display','inline-block').html('<script>var infolinks_pid = 2968036;var infolinks_wsid = 0;</script><script src="//resources.infolinks.com/js/infolinks_main.js"></script>');
    }

    $('[data-toggle="tooltip"]').tooltip(); //Set tooltips
    $('[name=notify_url]').val('https://' + document.location.host + '/payment_notification/' + this.model.meta.id); //Set the paypal notify URL
  },
  loadNodeImages: function (_loadNodeImages) {
    function loadNodeImages() {
      return _loadNodeImages.apply(this, arguments);
    }

    loadNodeImages.toString = function () {
      return _loadNodeImages.toString();
    };

    return loadNodeImages;
  }(function () {
    loadNodeImages(this.$el);
  }),
  //Change this to a check password function
  checkPassword: function checkPassword() {
    debug()('Checking password');
    var self = this;
    if (self.model.get('passwordProtectOn')) {
      //moot point if you aren't protecting this
      var el = $('.passwordProtection');
      var pass = $(el).find('input[name="password"]').val();
      var checkingEl = el.find('.checking');
      var incorrectEl = el.find('.incorrect');
      var correctEl = el.find('.correct');

      checkingEl.hide();
      correctEl.hide();
      incorrectEl.hide();

      if (pass.length == 0) {
        return;
      }

      checkingEl.show(); //Show the loading element

      var needsToCheckWithServer = !window.APP_SETTINGS;
      debug()('Need to check password with server: ' + needsToCheckWithServer);

      var onCorrect = function onCorrect() {
        debug()('Password is correct');
        checkingEl.hide();
        incorrectEl.hide();
        correctEl.fadeIn();

        self.model.setLocals({ passwordProtectSuccess: true }); //you've already done the password stuff and gotten the data. Functionally we're done

        self.render(); //Actually render the stuff

        if (window.APP_SETTINGS) {
          var previewEl = APP_SETTINGS.$el.find('.previewPassword');
          previewEl.find('.showPasswordPreview').show();
          previewEl.find('.hidePasswordPreview').hide();
        }
      };
      var onIncorrect = function onIncorrect() {
        debug()('Password is incorrect');
        checkingEl.hide();
        incorrectEl.fadeIn();
      };

      if (needsToCheckWithServer) {
        var options = {
          url: '/apps/' + self.model.meta.id + '/content.json',
          data: {
            id: self.model.meta.id,
            password: pass,
            view_mode: typeof VIEW_MODE == 'undefined' ? '' : VIEW_MODE //For Wix
          },
          type: 'POST'
        };
        //debug()('ASrgle blargle', options);
        ajaxController(options, function (r) {
          if (r.success) {
            if (r.correct) {
              //var oldLocals = self.model.get('locals');//preserve locals
              //self.model.attributes = r.content;
              //self.model.attributes.locals = oldLocals;

              onCorrect();

              for (var key in r.content) {
                if (r.content[key] != 'undefined') {
                  self.model.set(key, r.content[key]);
                }
              }

              self.afterCorrectPassword();
            } else {
              onIncorrect();
            }
          } else {
            onIncorrect();
          }
        });
      } else {
        //if the array of passwords contains the given password
        if (self.model.get('passwords').map(function (el) {
          return el.password;
        }).indexOf(pass) != -1) {
          onCorrect();
        } else {
          onIncorrect();
        }
      }
    }
  },
  afterCorrectPassword: function afterCorrectPassword() {},
  //Post the width and height of the app to the powr.js script if it exists (also posts size to wix):
  postSizeToSocket: function postSizeToSocket() {
    var self = this;
    //debug()('Trying to post view size to socket');
    //Set height of iframe in wix
    if (window.HOST == 'wix') {
      debug()('Setting wix height');
      if (typeof WIX_HEIGHT_TIMER != 'undefined') {
        //Timer to avoid over calling, which resulting in pages hanging due to wix constantly calling TriggerChange events
        clearTimeout(WIX_HEIGHT_TIMER);
      }
      window.WIX_HEIGHT_TIMER = setTimeout(function () {
        var h = self.$el.outerHeight(true);
        Wix.setHeight(h);
      }, 200);

      ////Width hack for iframes in wix for mobile safari:
      //this.$el.parents('html, body').css({'max-width':Wix.Utils.getWidth()+"px"});
    } else {
      var w = this.$el.outerWidth(true); //Width including margin
      var h = this.$el.outerHeight(true); //Height including margin
      if ($('#editMe.design_2').length > 0 && $('#editMe.design_2').css('display') != 'none' && self.model.meta.app_namespace != 'notificationBar') {
        //For edit me design 2
        h += $('#editMe.design_2').outerHeight(true);
      }
      if ($('.js-editMeMenu').css('display') == 'block') {
        h += 100; //adding 100px for the edit button dropdown height so it isn't cut off
      }
      //if plugin overlay is active
      if ($('.js-unclaimed-watermark').css('display') == 'block') {
        h = Math.max(250, h); //makes sure the height of the iframe is big enough for the overlay
      }
      if (self.model.get('locals').parent_origin != undefined) {
        //Blogger ad:
        if ($('.adWrapper').length > 0) {
          h += $('.adWrapper').outerHeight(true);
        }
        var data = {
          width: w,
          height: h,
          iframe_index: self.model.get('locals').iframe_index
        };
        if (self.model.get('locals').postCss != undefined) {
          data.postCss = jQuery.extend(true, {}, self.model.get('locals').postCss);
          if ($('.js-editMeMenu').css('display') == 'block' && data.postCss.position != 'fixed') {
            data.postCss.height = '175px'; //adding 100px for the edit button dropdown height so it isn't cut off
          }
          if (self.model.get('locals').overridePostCssHeight) {
            data.postCss.height = h;
          }
        }
        if (self.model.get('locals').postMessage != undefined) {
          data.postMessage = self.model.get('locals').postMessage;
        }
        var r = {
          message: 'updateSize',
          data: data
        };
        parent.postMessage(JSON.stringify(r), self.model.get('locals').parent_origin);
      } else if (typeof APP_SOCKET != 'undefined') {
        //This is for old XDM (still used on joomla)
        debug()('App socket exists');
        APP_SOCKET.postMessage(JSON.stringify({ action: 'updateSize', width: w, height: h }));
      }
    }
  },
  loadFonts: function loadFonts() {
    var self = this;
    //Try to find fonts
    var fonts = [];
    for (var i in this.model.attributes) {
      var currentAttribute = this.model.attributes[i];
      if (i.search('FontFamily') > -1) {
        fonts.push(currentAttribute);
      }
      //Look for NESTED fonts:
      if ((typeof currentAttribute === 'undefined' ? 'undefined' : _typeof(currentAttribute)) == 'object') {
        for (var key in currentAttribute) {
          var currentSubAttribute = currentAttribute[key];
          if (key.search('FontFamily') > -1) {
            fonts.push(currentSubAttribute);
          }
          if ((typeof currentSubAttribute === 'undefined' ? 'undefined' : _typeof(currentSubAttribute)) == 'object') {
            for (var subkey in currentSubAttribute) {
              if (subkey.search('FontFamily') > -1) {
                fonts.push(currentSubAttribute[subkey]);
              }
            }
          }
        }
      }
    }
    //debug()('Fonts are : ',fonts);
    if (fonts.length > 0) {
      Object(_helpers_google_fonts__WEBPACK_IMPORTED_MODULE_0__["loadFontsFromGoogle"])(fonts); //Load fonts from google
    }
  },
  afterRender: function afterRender() {},
  applyPasswordStyle: function applyPasswordStyle() {
    //default. Can be overridden in a specific model
    this.setStandardBackground($('.passwordProtection'));
  },
  //Set standard background settings
  setStandardBackground: function setStandardBackground(backgroundElements) {
    if (typeof backgroundElements == 'undefined') {
      var el = this.$el.children(); //Assume background is child of the view element
    } else {
      var el = backgroundElements;
    }
    var m = this.model;

    if (m.get('backgroundShadow') != 'none') {
      el.toggleClass(m.get('backgroundShadow'), true);
    }
    if (m.get('backgroundGradient') == true) {
      var _el$css;

      el.css((_el$css = {
        background: '-webkit-linear-gradient(' + m.get('backgroundStartColor') + ',' + m.get('backgroundEndColor') + ')'
      }, _defineProperty(_el$css, 'background', '-o-linear-gradient(' + m.get('backgroundStartColor') + ',' + m.get('backgroundEndColor') + ')'), _defineProperty(_el$css, 'background', '-moz-linear-gradient(' + m.get('backgroundStartColor') + ',' + m.get('backgroundEndColor') + ')'), _defineProperty(_el$css, 'background', 'linear-gradient(' + m.get('backgroundStartColor') + ',' + m.get('backgroundEndColor') + ')'), _defineProperty(_el$css, 'border-color', m.get('backgroundBorderColor')), _defineProperty(_el$css, 'border-radius', m.get('backgroundBorderRadius')), _defineProperty(_el$css, 'border-width', m.get('backgroundBorderWidth')), _defineProperty(_el$css, 'border-style', 'solid'), _el$css));
    } else {
      el.css({
        background: m.get('backgroundColor'),
        'border-color': m.get('backgroundBorderColor'),
        'border-radius': m.get('backgroundBorderRadius'),
        'border-width': m.get('backgroundBorderWidth'),
        'border-style': 'solid'
      });
    }
  },
  setStandardWidth: function setStandardWidth(el) {
    if (this.model.get('pluginWidth') === '1000px') {
      el.css({ maxWidth: '100%' });
    } else {
      el.css({ maxWidth: this.model.get('pluginWidth') });
    }
  },
  fitText: function (_fitText) {
    function fitText() {
      return _fitText.apply(this, arguments);
    }

    fitText.toString = function () {
      return _fitText.toString();
    };

    return fitText;
  }(function () {
    var self = this;
    fitText(this.$el.find('.fitText'));
    this.afterFitText(); //Call afterFitText()
  }),

  afterFitText: function afterFitText() {}
}));

function appViewAnalytics() {
  if (!window.APP_MODEL) {
    setTimeout(appViewAnalytics, 1500);
    return;
  }
  //ANALYTICS
  (function (i, s, o, g, r, a, m) {
    i['GoogleAnalyticsObject'] = r;
    i[r] = i[r] || function () {
      (i[r].q = i[r].q || []).push(arguments);
    }, i[r].l = 1 * new Date();
    a = s.createElement(o), m = s.getElementsByTagName(o)[0];
    a.async = 1;
    a.src = g;
    m.parentNode.insertBefore(a, m);
  })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
  try {
    ga('create', 'UA-86251018-1', 'auto', 'powr_apps', { 'sampleRate': 5 }); //POWR APPS Google analytics
    ga('powr_apps.send', 'pageview', '/' + APP_MODEL.meta.id);
  } catch (e) {
    console.log('Trouble loading google analytics');
  }
}

$(window).load(function () {
  //Slow stuff we dont want to affect load
  if (typeof initializeAhoy == 'function') {
    initializeAhoy();
  }
  appViewAnalytics();
});

