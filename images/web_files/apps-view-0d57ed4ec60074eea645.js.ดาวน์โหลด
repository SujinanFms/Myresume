var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

function _possibleConstructorReturn(self, call) { if (!self) { throw new ReferenceError("this hasn't been initialised - super() hasn't been called"); } return call && (typeof call === "object" || typeof call === "function") ? call : self; }

function _inherits(subClass, superClass) { if (typeof superClass !== "function" && superClass !== null) { throw new TypeError("Super expression must either be null or a function, not " + typeof superClass); } subClass.prototype = Object.create(superClass && superClass.prototype, { constructor: { value: subClass, enumerable: false, writable: true, configurable: true } }); if (superClass) Object.setPrototypeOf ? Object.setPrototypeOf(subClass, superClass) : subClass.__proto__ = superClass; }



/*
Component: PowrTabs
Usage: Used to create tabs - eg: nav tab
Note: This does not currently wrap the component around settings component wrapper - if we need to use this for premium components, please add that.

Required Props:
name: Unique name (ref) for the react component
options: List fo tabs shown - {key: value} key is unique identifier, value is displayed
handleTabClick: Callback to handle when a tab is selected

Optional Props:

Example:
<PowrTabs
  name="navTab" options={[<list of tabs>]} handleTabClick={this.handleTabClick}
/>

Author: Praneeta
*/

var PowrTabs = function (_React$Component) {
  _inherits(PowrTabs, _React$Component);

  function PowrTabs(props) {
    _classCallCheck(this, PowrTabs);

    var _this = _possibleConstructorReturn(this, (PowrTabs.__proto__ || Object.getPrototypeOf(PowrTabs)).call(this, props));

    _this.handleClick = function (key) {
      _this.setState({
        activeTab: key
      });
      _this.props.handleTabClick(key);
    };

    _this.state = {
      activeTab: _.first(props.options).key
    };
    return _this;
  }

  _createClass(PowrTabs, [{
    key: 'render',
    value: function render() {
      var _this2 = this;

      var items = this.props.options.map(function (item, index) {
        return react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement(PowrTabItem, {
          value: item.value,
          key: item.key,
          name: item.key,
          onClick: _this2.handleClick,
          selected: _this2.state.activeTab == item.key
        });
      });
      return react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement(
        'ul',
        { ref: this.props.namespace, className: 'tab-component-controls-list ' + this.props.namespace },
        items
      );
    }
  }]);

  return PowrTabs;
}(react__WEBPACK_IMPORTED_MODULE_0___default.a.Component);

var PowrTabItem = function PowrTabItem(props) {
  var handleTabItemClick = function handleTabItemClick() {
    props.onClick(props.name);
  };
  return react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement(
    'li',
    { className: 'tab-component-controls-list__item ' + (props.selected && 'active'), onClick: handleTabItemClick },
    react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement(
      'a',
      { className: 'tab-component-controls-list__link' },
      props.value
    )
  );
};

/* harmony default export */ __webpack_exports__["default"] = (PowrTabs);

