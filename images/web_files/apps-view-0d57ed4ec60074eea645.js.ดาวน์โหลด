  // },

  //To set locals, should be called like:
  // this.model.setLocals({key: val, key2: val2})
  setLocals: function setLocals(opts) {
    var locals = this.get('locals'); //Get locals
    if (locals == undefined) {
      //If not defined, create an empty object
      locals = {};
    }
    for (var i in opts) {
      locals[i] = opts[i];
    }
    this.set({ locals: locals }, { silent: true }); //Set the locals
  },

  createUnublishedContentCookie: function createUnublishedContentCookie() {
    var appType = this.meta.app_type;
    var appId = this.meta.id;
    try {
      var unpublishedContent = JSON.parse(getCookie('unpublished_content'));
    } catch (e) {
      var unpublishedContent = {};
    }
    unpublishedContent[appType] = appId;
    setCookie('unpublished_content', JSON.stringify(unpublishedContent), 365);
  },

  destroyUnublishedContentCookie: function destroyUnublishedContentCookie() {
    var appType = this.meta.app_type;
    try {
      var unpublishedContent = JSON.parse(getCookie('unpublished_content'));
    } catch (e) {
      var unpublishedContent = {};
    }
    delete unpublishedContent[appType];
    setCookie('unpublished_content', JSON.stringify(unpublishedContent), 365);
  },

  //For recovery of saved changes to plugins...combining in isDirty with it...
  isDirtyWithCookie: function isDirtyWithCookie() {
    debug()('Creating unpublished content cookie');
    this.createUnublishedContentCookie();
    this.setLocals({ isDirty: true, isChangedFromDefault: true });
    window.updateEditTopbarState && window.updateEditTopbarState('hasUnpublishedChanges', true); // tell edit top bar that there are unpublished changes
  },

  isDirtyNoMoreWithCookie: function isDirtyNoMoreWithCookie() {
    debug()('Destroying unpublished content cookie');
    this.destroyUnublishedContentCookie();
    this.setLocals({ isDirty: false }); //Set model is no longer dirty
    window.updateEditTopbarState && window.updateEditTopbarState('hasUnpublishedChanges', false); // tell edit top bar that changes are published
  },

  setPresets: function setPresets(color_presets, font_presets) {
    // debug()("Model setting presets");
    // var preset_map = this.presets();
    // var updated_attributes = {};
    // for(var p in preset_map){
