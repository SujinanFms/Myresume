var _slicedToArray = function () { function sliceIterator(arr, i) { var _arr = []; var _n = true; var _d = false; var _e = undefined; try { for (var _i = arr[Symbol.iterator](), _s; !(_n = (_s = _i.next()).done); _n = true) { _arr.push(_s.value); if (i && _arr.length === i) break; } } catch (err) { _d = true; _e = err; } finally { try { if (!_n && _i["return"]) _i["return"](); } finally { if (_d) throw _e; } } return _arr; } return function (arr, i) { if (Array.isArray(arr)) { return arr; } else if (Symbol.iterator in Object(arr)) { return sliceIterator(arr, i); } else { throw new TypeError("Invalid attempt to destructure non-iterable instance"); } }; }();

var ENV = window.ENVIRONMENT;




/* harmony default export */ __webpack_exports__["default"] = (Backbone.Model.extend({
  presets: function presets() {
    return {};
  },
  baseDefaults: function baseDefaults() {
    return {};
  },
  afterMount: function afterMount() {},
  defaults: function defaults() {
    var d = this.baseDefaults();
    if (META.default_overrides) {
      for (var i in META.default_overrides) {
        if (d[i] !== undefined) {
          if (HOST == 'wix' && (i.indexOf('FontFamily') > -1 || i.indexOf('WixColor') > -1)) {
            continue;
          }
          d[i] = META.default_overrides[i];
        }
      }
    }
    if (HOST == 'wix') {
      d['pluginWidth'] = '1000px';
      if (META.app_namespace == 'photoEditor') {
        d['imageUrl'] = d['imageUrl'].replace('.jpg', '_wix.jpg'); //Different image for wix
      } else if (META.app_namespace == 'map') {
        var first_location = d['locations'][0];
        d['locations'] = [];
        d['locations'].push(first_location);
      }
    }
    return d;
  },
  urlRoot: '/apps/', //Url root for app apps
  initialize: function initialize() {
    //Set defaults this way
    if (!this.hasOwnProperty('isPreset')) {
      this.set({ isPreset: true }, { silent: true }); //App behaves as if it starts with a preset. isPreset is set to the model.
    }
    this.setLocals({ render_style_only: null }); //Initialize render_style_only to null
    if (this.afterInitialize) this.afterInitialize();

    // Note : this.meta is not defined yet, so we are using window.META instead
    // Note : Commenting out chameleon code for now
    // Ref: https://powrtrak.atlassian.net/browse/PM-5333

    if (!!META.chameleon_url && !META.saved) {
      debugger;
      this.getChameleon(META.chameleon_url);
    } //else if (getURLParameter('request_url')) {
    //   this.getChameleon(getURLParameter('request_url'));
    // }
  },
  //A safer way to trigger change that will work in wix instead of just triggering the model change.
  triggerChange: function triggerChange() {
    var cookie_flag = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : true;

    var self = this;

    debug()('TriggerChange called');
    //Need to manually trigger change for wix
    if (HOST == 'wix' && typeof APP_SETTINGS != 'undefined') {
      //Trigger the view to re-render
      Wix.Settings.triggerSettingsUpdatedEvent(this.attributes, Wix.Utils.getOrigCompId());
      this.setLocals({ render_style_only: null }); //Return render style to null here because the model changed in the view is not synced back to the settings
    } else if (HOST == 'weebly' || typeof INPAGE != 'undefined' && INPAGE) {
      localStorage.setItem('powr_data_' + self.meta.id, JSON.stringify({ content: self.attributes, meta: self.meta }));
    } else if (META.external_id_type == 'cloudflare') {
      localStorage.setItem('powr_data_' + self.meta.id, JSON.stringify({ content: self.attributes, meta: self.meta }));

      self.trigger('change');
    } else {
      self.trigger('change');
    }

    if (cookie_flag) {
      self.isDirtyWithCookie();
    }
  },

  updateWixStyles: function updateWixStyles() {
    var self = this;
    if (typeof WIX_COLOR_CHANGE_TIMER == 'undefined') {
      self._updateWixStyles();
      window.WIX_COLOR_CHANGE_TIMER = setTimeout(function () {}, 0);
    } else {
      clearTimeout(WIX_COLOR_CHANGE_TIMER);
      //Called directly in wix views
      //Set the wix components
      window.WIX_COLOR_CHANGE_TIMER = setTimeout(function () {
        self._updateWixStyles();
      }, 500);
    }
  },
  _updateWixStyles: function _updateWixStyles() {
    var self = this;

    function parseWixUrl(str) {
      //debug()("parseWixUrl called with string "+str);
      if (typeof Wix != 'undefined') {
        str = str.replace(/^http.+?(static\.wix\.com|static\.wixstatic\.com)\/media\/.+?srz$/i, function (val) {
          //This hits the normal case of strings
          //debug()("Replacing val: "+val);
          val = val.substring(val.indexOf('media/') + 6);
          val = val.substring(0, val.indexOf('.') + 4);
          val = val.replace('.jpe', '.jpeg');
          return Wix.Utils.Media.getResizedImageUrl(val, 1200, 1200);
        });
        str = str.replace(/src="http.+?(static\.wix\.com|static\.wixstatic\.com)\/media\/.+?srz"/gi, function (val) {
          //This hits image links inside of strings
          //debug()("Replacing val: "+val);
          val = val.replace(/"/g, ''); //Remove quotes
          val = val.replace('src=', ''); //Remove src=
          val = val.substring(val.indexOf('media/') + 6);
          val = val.substring(0, val.indexOf('.') + 4);
          val = val.replace('.jpe', '.jpeg');
          return 'src="' + Wix.Utils.Media.getResizedImageUrl(val, 1200, 1200) + '"';
        });
        return str;
      } else {
        return str;
      }
    }

    for (var key in self.attributes) {
      //Set colors
      if (key.search('ColorWix') > -1) {
        //If a wix color, then set corresponding normal color:
        var wix_value = self.attributes[key]; //The value in wix eg 'color-1'
        if (wix_value == null || wix_value.length == 0) {
          //IF no value then continue
          continue;
        }
        var normal_color_key = key.replace('Wix', ''); //The key without wix eg 'backgroundColor'
        var wixSiteColors = Wix.Styles.getSiteColors();
        // debug()("Wix value", wix_value);
        var wixColor = _.findWhere(wixSiteColors, { reference: wix_value });
        // debug()("Wix color", wixColor, wixSiteColors);
        var normal_color_value = wixColor && Object(_modules_react_components_powr_color_picker__WEBPACK_IMPORTED_MODULE_2__["makeColorObject"])(wixColor.value);
        if (normal_color_value != undefined) {
          //Prevent garbage from being set
          // debug()("Wix styles setting color. Wix value is "+wix_value+"; normal color key: "+normal_color_key+"; normal color value: "+normal_color_value);
          var originalColor = Object(_modules_react_components_powr_color_picker__WEBPACK_IMPORTED_MODULE_2__["makeColorObject"])(self.attributes[normal_color_key]);
          self.attributes[normal_color_key] = Object(_modules_react_components_powr_color_picker__WEBPACK_IMPORTED_MODULE_2__["makeColorString"])(normal_color_value); //Set the value
          if (originalColor.rgb.a) {
            normal_color_value.rgb.a = originalColor.rgb.a;
            self.attributes[normal_color_key] = Object(_modules_react_components_powr_color_picker__WEBPACK_IMPORTED_MODULE_2__["makeColorString"])(normal_color_value);
          }
        }
      }

      //Set fonts
      //                //TODO: THis doesn't work yet, because we need the index to somehow map to the font. Not sure if they always will appear in order
      //                if(key.search('FontFamilyWix') > -1){
      //                    var wix_value = this.attributes[key]; //The value in wix eg '4'
      //                    if(wix_value == null || wix_value.length==0){ //IF no value then continue
      //                        continue;
      //                    }
      //                    var normal_model_key = key.replace('Wix',''); //The key without wix eg 'backgroundColor'
      //                    var normal_model_value = $('.wix-style-reference .'+wix_value).css('fontFamily'); //Get the actual value
      //                    if(normal_model_value != undefined && normal_model_value.length > 0){//Prevent garbage from being set
      //                        this.attributes[normal_model_key]=normal_model_value; //Set the value
      //                    }
      //                }

      //Set images
      //debug()("Current value is ", self.attributes[key]);
      if (typeof self.attributes[key] == 'string') {
        self.attributes[key] = parseWixUrl(self.attributes[key]);
      } else {
        for (var subkey in self.attributes[key]) {
          if (typeof self.attributes[key][subkey] == 'string') {
            self.attributes[key][subkey] = parseWixUrl(self.attributes[key][subkey]);
          } else {
            for (var subsubkey in self.attributes[key][subkey]) {
              if (typeof self.attributes[key][subkey][subsubkey] == 'string') {
                self.attributes[key][subkey][subsubkey] = parseWixUrl(self.attributes[key][subkey][subsubkey]);
              } else {
                for (var subsubsubkey in self.attributes[key][subkey][subsubkey]) {
                  if (typeof self.attributes[key][subkey][subsubkey][subsubsubkey] == 'string') {
                    self.attributes[key][subkey][subsubkey][subsubsubkey] = parseWixUrl(self.attributes[key][subkey][subsubkey][subsubsubkey]);
                  } else {
                    for (var subsubsubsubkey in self.attributes[key][subkey][subsubkey][subsubsubkey]) {
                      if (typeof self.attributes[key][subkey][subsubkey][subsubsubkey][subsubsubsubkey] == 'string') {
                        self.attributes[key][subkey][subsubkey][subsubsubkey][subsubsubsubkey] = parseWixUrl(self.attributes[key][subkey][subsubkey][subsubsubkey][subsubsubsubkey]);
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
    if (typeof APP_VIEW != 'undefined') {
      debug()('Rendering view from _updateWixStyles');
      APP_VIEW.render();
    }
  },

  loadChameleonToModel: function loadChameleonToModel() {},

  mapChameleonToModel: function mapChameleonToModel(appModel, mapper) {
    try {
      var _iteratorNormalCompletion = true;
      var _didIteratorError = false;
      var _iteratorError = undefined;

      try {
        for (var _iterator = Object.entries(mapper)[Symbol.iterator](), _step; !(_iteratorNormalCompletion = (_step = _iterator.next()).done); _iteratorNormalCompletion = true) {
          var _ref = _step.value;

          var _ref2 = _slicedToArray(_ref, 2);

          var key = _ref2[0];
          var value = _ref2[1];

          var _value$split = value.split('.'),
              _value$split2 = _slicedToArray(_value$split, 2),
              first = _value$split2[0],
              second = _value$split2[1];

          var chameleon = appModel.meta.chameleon[first][second];
          var attribute = appModel.get(key);
          var obj = {};

          if (attribute === chameleon || chameleon === null) return;

          obj[key] = chameleon;
          appModel.set(obj, { silent: true });
        }
      } catch (err) {
        _didIteratorError = true;
        _iteratorError = err;
      } finally {
        try {
          if (!_iteratorNormalCompletion && _iterator.return) {
            _iterator.return();
          }
        } finally {
          if (_didIteratorError) {
            throw _iteratorError;
          }
        }
      }
    } catch (error) {
      debug()(error);
    }

    if (appModel.hasChanged()) appModel.triggerChange();
  },

  // getChameleon: function(url) {
