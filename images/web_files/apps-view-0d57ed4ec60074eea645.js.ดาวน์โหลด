"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "getNextUpgradeStatus", function() { return getNextUpgradeStatus; });
var plans = ['free', 'premium', 'pro', 'enterprise'];

var UpgradeHelper = {
  isUpgradeRequired: function isUpgradeRequired(userStatus, requiredStatus, limits, currentLimitsCount) {
    if (limits && currentLimitsCount) {
      return currentLimitsCount >= limits[userStatus];
    }
    return plans.indexOf(userStatus) < plans.indexOf(requiredStatus);
  }
};

var getNextUpgradeStatus = function getNextUpgradeStatus(userStatus) {
  return plans[plans.indexOf(userStatus) + 1] || 'enterprise';
};

/* harmony default export */ __webpack_exports__["default"] = (UpgradeHelper);

