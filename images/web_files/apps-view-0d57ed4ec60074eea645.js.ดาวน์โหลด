window.setMultiToggle = function ($multiToggle) {
  var $input = $multiToggle.siblings('.multiToggleInput');
  var clicked_value = $multiToggle.data('value');
  var input_value = $input.val();
  if (clicked_value != input_value) {
    $input.val(clicked_value).change();
    $multiToggle.addClass('active');
    $multiToggle.siblings('.multiToggle').removeClass('active');
  }
};

