//Parse a query string to an object
window.parseQuery = function (a) {
  if (a == '') return {};
  a = a.split('&');
  var b = {};
  for (var i = 0; i < a.length; ++i) {
    var p = a[i].split('=', 2);
    if (p.length == 1) b[p[0]] = '';else b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, ' '));
  }
  return b;
};

//Test if something is null, undefined, or empty. Because the underscore window.sucks.
window.isEmpty = function (val) {
  if (typeof val == 'number') {
    return false;
  }
  return _.isEmpty(val);
};

window.setCookie = function (c_name, value, exdays, use_local_storage_backup) {
  var exdate = new Date();
  ex_timestamp = exdate.setDate(exdate.getDate() + exdays); //This returns the expiration timestamp in unixtime 1462987106594
  var c_value = escape(value) + (exdays == null ? '' : '; expires=' + exdate.toUTCString() + ';path=/');
  document.cookie = c_name + '=' + c_value;
  //Cookie has been attempted to be set. If user_local_storage_backup, then use local storage if cookie wasn't set
  if (getCookie(c_name) == undefined && typeof use_local_storage_backup != 'undefined' && use_local_storage_backup) {
    try {
      localStorage.setItem(c_name, JSON.stringify({ value: value, expiration: ex_timestamp }));
    } catch (e) {
      debug()('Local storage error:', e);
    }
  }
};

window.validateEmail = function (emailInput) {
  // TODO replace with global email validator?
  var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
  if (re.test(emailInput)) {
    return true;
  } else {
    return false;
  }
};

//Return cookie value of given name
window.getCookie = function (c_name, use_local_storage_backup) {
  var i,
      x,
      y,
      ARRcookies = document.cookie.split(';');
  for (i = 0; i < ARRcookies.length; i++) {
    x = ARRcookies[i].substr(0, ARRcookies[i].indexOf('='));
    y = ARRcookies[i].substr(ARRcookies[i].indexOf('=') + 1);
    x = x.replace(/^\s+|\s+$/g, '');
    if (x == c_name) {
      var cookie_value = unescape(y);
      break;
    }
  }
  if (cookie_value == undefined && typeof use_local_storage_backup != 'undefined' && use_local_storage_backup) {
    //Get from local storage
    var data = localStorage.getItem(c_name);
    if (data == undefined || data == null || data == 'null') {
      return;
    }
    data = JSON.parse(data);
    //Deal with expiration
    if (data.expiration < new Date().getTime()) {
      localStorage.removeItem(c_name); //Destroy data
      return;
    } else {
      return data.value;
    }
  } else {
    return cookie_value;
  }
};

window.originalQuery = function () {
  try {
    return window.location.search.replace('?', ''); //window.location is sometimes undefined according to bugsnag
  } catch (err) {
    return '';
  }
};

window.getURLParameter = function (name) {
  return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search) || [, ''])[1].replace(/\+/g, '%20')) || null;
};

window.pluginIcon = function (app_type) {
  //Same as plugin_icon helper but f S
  return 'https://s3-us-west-1.amazonaws.com/powr/plugins/' + app_type + '/318x318.jpg';
};

window.truncate = function (str, length) {
  return str.length > length ? str.substring(0, length - 3) + '...' : str;
};

