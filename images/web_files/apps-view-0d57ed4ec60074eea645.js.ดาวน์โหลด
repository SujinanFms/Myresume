/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "loadRecentFonts", function() { return loadRecentFonts; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "addRecentFont", function() { return addRecentFont; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "loadFontsFromGoogle", function() { return loadFontsFromGoogle; });
/* harmony import */ var _helpers_constants__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(630);
/* harmony import */ var util__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(632);
/* harmony import */ var util__WEBPACK_IMPORTED_MODULE_1___default = /*#__PURE__*/__webpack_require__.n(util__WEBPACK_IMPORTED_MODULE_1__);



var loadRecentFonts = function loadRecentFonts() {
  try {
    var fontsRaw = localStorage.getItem(_helpers_constants__WEBPACK_IMPORTED_MODULE_0__["POWR_FONT_FAMILY_KEY"]);
    var recentFonts = fontsRaw ? JSON.parse(fontsRaw) : [];
    return recentFonts;
  } catch (e) {
    Object(util__WEBPACK_IMPORTED_MODULE_1__["debug"])()(e);
    return [];
  }
};

var addRecentFont = function addRecentFont(newFontName) {
  var recentFonts = loadRecentFonts();
  if (recentFonts.indexOf(newFontName) === -1) {
    var fontCount = recentFonts.unshift(newFontName);
    if (fontCount > 3) {
      recentFonts.pop();
    }
  } else {
    recentFonts = recentFonts.filter(function (recentFont) {
      return recentFont !== newFontName;
    });
    recentFonts.unshift(newFontName);
  }
  try {
    localStorage.setItem(_helpers_constants__WEBPACK_IMPORTED_MODULE_0__["POWR_FONT_FAMILY_KEY"], JSON.stringify(recentFonts));
    return recentFonts;
  } catch (e) {
    Object(util__WEBPACK_IMPORTED_MODULE_1__["debug"])()(e);
    return [];
  }
};

//List of websafe fonts to ignore:
var ignoreFonts = ['Courier New', 'Georgia', 'Palatino Linotype', 'Times New Roman', 'Arial', 'Arial Black', 'Comic Sans', 'Tahoma', 'Verdana', 'Lucida Sans Unicode', 'Lucida Console', 'Comic sans ms', 'Impact'];

/*Google font loader*/
//Can pass in an array of fonts to be loaded at any time on the page;
//EG: arr = ["Signika","Fredericka the Great"]
//Will convert dashes to spaces, eg: this is ok too: ["Signika","Fredericka-the-Great"]
//To apply these fonts, simply add to the element: "font-family: "Fredericka the Great"

var loadFontsFromGoogle = function loadFontsFromGoogle(arr) {
  //Remove any empty elements, ignored fonts, and fonts with commas in them (which are lists that come from WIx)
  var newArr = [];
  for (var i in arr) {
    if (arr[i].length != 0) {
      if ($.inArray(arr[i], ignoreFonts) == -1 && arr[i].search(',') == -1) {
        //Dont include ignored fonts; Dont include fonts with commas
        newArr.push(arr[i]);
      }
    }
  }
  arr = newArr;
  //Replace dashes with spaces:
  for (var _i = 0; _i < arr.length; _i++) {
    arr[_i] = replaceAll('-', ' ', arr[_i]);
  }
  //Create global fonts if they don't exist:
  if (typeof ALL_FONTS == 'undefined') {
    window.ALL_FONTS = []; //Global array of fonts
  }
  var new_fonts = false; //Flag whether new fonts are detected
  for (var _i2 in arr) {
    var current_font = arr[_i2];
    if ($.inArray(current_font, ALL_FONTS) == -1) {
      //If a new font is detected
      ALL_FONTS.push(current_font);
      new_fonts = true;
    }
  }
  if (new_fonts == false) {
    //debug()("No new fonts detected");
    return;
  }
  if (typeof WebFont != 'undefined') {
    //debug()("Calling webfont for fonts: ",arr);
    WebFont.load({
      google: { families: arr }
    });
  }
};

