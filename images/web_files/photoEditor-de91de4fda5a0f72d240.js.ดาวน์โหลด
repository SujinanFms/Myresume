$(function () {
  photoEditorBaseModel = appModel.extend({
    baseDefaults: function baseDefaults() {
      var d = {
        imageUrl: 'https://s3-us-west-1.amazonaws.com/powr/images/default.jpg',
        caption: '',
        link: '',
        rightClick: true,

        linkBehavior: '_blank',

        //Watermark
        showWatermark: false,
        watermarkSymbol: 'copyright',
        watermarkUrl: 'https://s3-us-west-1.amazonaws.com/powr/images/default.jpg',
        watermarkOpacity: 0.5,
        watermarkScale: '25%',
        watermarkTop: '73%',
        watermarkLeft: '73%',

        //Filters:
        filterGrayscale: 0,
        filterSepia: 0,
        filterSaturate: 1,
        filterHueRotate: '0deg',
        filterInvert: 0,
        filterOpacity: 1,
        filterBrightness: 1,
        filterContrast: 1,
        filterBlur: '0px',

        //Filter animation:
        animation: 'none',
        hoverEffect: 'none',
        filterAnimationTime: 500,

        //Not quite Standard Background
        //backgroundColor: '#fff',
        backgroundBorderColor: '#ccc',
        backgroundBorderRadius: '3%',
        backgroundBorderWidth: '0px',
        backgroundShadow: 'softDarkShadow',
        pluginWidth: '500px',

        // Captions
        captionBackgroundColor: 'rgba(51, 51, 51, 0.75)',
        captionFontFamily: 'Open Sans',
        captionFontSize: '12px',
        captionFontColor: '#EEE',
        captionAlign: 'center',
        captionPosition: 'bottom', //Top or bottom

        passwordProtectOn: false,
        customCSS: '',
        customJS: ''
      };
      if (HOST == 'wix') {
        try {
          var body_font = Wix.Styles.getSiteTextPresets()['Body-M'].fontFamily;
          d['captionFontFamily'] = body_font; //Border color
        } catch (err) {}
        d['backgroundBorderColorWix'] = 'color-2'; //Border color
        d['captionBackgroundColorWix'] = 'color-1'; //App background
        d['captionFontColorWix'] = 'color-4'; //Running text
      }
      return d;
    }
  });
  photoEditorAppModel = photoEditorBaseModel;
  imageResizerAppModel = photoEditorBaseModel;
  photoFilterAppModel = photoEditorBaseModel;
  photoWatermarkAppModel = photoEditorBaseModel;

  photoEditorAppSettings = appSettings.extend({
    afterInitialize: function afterInitialize() {
      var self = this;
      var m = this.model;

      //Only show animation if it is changed:
      this.$el.on('change reactUpdateRequest', '[name="filterAnimationTime"]', function () {
        self.model.setLocals({ showAnimation: true });
        setTimeout(function () {
          self.model.setLocals({ showAnimation: false });
        }, 2000);
      });

      this.$el.on('change reactUpdateRequest', '[name="filterPreset"]', function () {
        //Set defaults, then override in switch below.
        var filterGrayscale = 0;
        var filterSepia = 0;
        var filterSaturate = 1;
        var filterHueRotate = '0deg';
        var filterInvert = 0;
        var filterOpacity = 1;
        var filterBrightness = 1;
        var filterContrast = 1;
        var filterBlur = '0px';
        switch ($(this).val()) {
          case 'clarendon':
            filterSepia = 0.15;
            filterContrast = 1.25;
            filterBrightness = 1.25;
            filterHueRotate = '5deg';
            break;
          case 'gingham':
            filterContrast = 1.1;
            filterBrightness = 1.1;
            break;
          case 'inkwell':
            filterBrightness = 1.25;
            filterContrast = 0.85;
            filterGrayscale = 1;
            break;
          case 'juno':
            filterSepia = 0.35;
            filterContrast = 1.15;
            filterBrightness = 1.15;
            filterSaturate = 1.8;
            break;
          case 'lark':
            filterSepia = 0.25;
            filterContrast = 1.2;
            filterBrightness = 1.3;
            filterSaturate = 1.25;
            break;
          case 'valencia':
            filterSepia = 0.25;
            filterContrast = 1.1;
            filterBrightness = 1.1;
            break;
          case 'kelvin':
            filterSepia = 0.15;
            filterContrast = 1.5;
            filterBrightness = 1.1;
            filterHueRotate = '-10deg';
            break;
          case 'skyline':
            filterSepia = 0.15;
            filterContrast = 1.25;
            filterBrightness = 1.25;
            filterSaturate = 1.2;
            break;
        }
        self.$el.find('[name="filterGrayscale"]').val(filterGrayscale).trigger('forceUpdateComponent');
        self.$el.find('[name="filterSepia"]').val(filterSepia).trigger('forceUpdateComponent');
        self.$el.find('[name="filterSaturate"]').val(filterSaturate).trigger('forceUpdateComponent');
        self.$el.find('[name="filterHueRotate"]').val(filterHueRotate).trigger('forceUpdateComponent');
        self.$el.find('[name="filterInvert"]').val(filterInvert).trigger('forceUpdateComponent');
        self.$el.find('[name="filterOpacity"]').val(filterOpacity).trigger('forceUpdateComponent');
        self.$el.find('[name="filterBrightness"]').val(filterBrightness).trigger('forceUpdateComponent');
        self.$el.find('[name="filterContrast"]').val(filterContrast).trigger('forceUpdateComponent');
        self.$el.find('[name="filterBlur"]').val(filterBlur).trigger('forceUpdateComponent');
      });
    },
    afterUpdate: function afterUpdate() {
      var self = this;
      this.updateConditionals();
      this.model.triggerChange(); //For style render
    },
    afterRender: function afterRender() {
      this.updateConditionals();
    },
    css3FilterFeatureDetect: function css3FilterFeatureDetect() {
      //Remove false positives from old browsers:
      if (false == (document.documentMode === undefined || document.documentMode > 9)) {
        //non-IE browsers, including ancient IEs OR IE compatibility mode
        return false;
      }
      var isValid = false;
      var el = document.createElement('div'); //creating an element dynamically
      el.style.cssText = '-webkit-filter: blur(2px)'; //try adding webkit filter
      if (el.style.length > 0) {
        //See if it works
        isValid = true;
      }
      el.style.cssText = 'filter: blur(2px)'; //adding regular filter
      if (el.style.length > 0) {
        //See if it works
        isValid = true;
      }
      //Remove element and return
      $(el).remove();
      return isValid;
    },
    updateConditionals: function updateConditionals() {
      var self = this;
      //Set whether css3 filters are available (true or false)
      if (this.model.get('locals').cssFilters == undefined) {
        this.model.setLocals({ cssFilters: self.css3FilterFeatureDetect() });
      }
      //CSS_FILTERS =  self.css3FilterFeatureDetect();

      if (this.model.get('locals').cssFilters) {
        this.$el.find('.filters').show();
        this.$el.find('.noFilters').hide();
      } else {
        this.$el.find('.filters').hide();
        this.$el.find('.noFilters').show();
      }

      if (this.model.get('caption')) {
        this.$el.find('#captionAccordion').show();
      } else {
        this.$el.find('#captionAccordion').hide();
      }
      if (this.model.get('showWatermark')) {
        this.$el.find('.watermarkDetails').show();
      } else {
        this.$el.find('.watermarkDetails').hide();
      }
      if (this.model.get('watermarkSymbol') == 'custom') {
        this.$el.find('.customWatermark').show();
      } else {
        this.$el.find('.customWatermark').hide();
      }
    }
  });
  photoEditorAppView = appView.extend({
    afterInitialize: function afterInitialize() {
      var self = this;
    },
    setMetaContent: function setMetaContent() {
      //The default meta section for an app. Note this is HTML meta
      var description = this.model.get('caption');
      this.writeMetaContent(description);
    },
    afterRender: function afterRender() {
      var m = this.model;
      var el = this.$el.find('.imageApp');
      //Animation
      if (m.get('locals').showAnimation) {
        var animationTime = parseInt(m.get('filterAnimationTime'));
      } else {
        var animationTime = 0;
      }
      el.find('.imageFilter, .watermark').fadeIn(animationTime);
    }
  });
  __webpack_require__(1101);
});
